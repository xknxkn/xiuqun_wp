	<texts>
		<part part_index="0" name="Look it up" page="75">
			<record rec_id="0" rec_time="25" sound="nj_08a_05.mp3">
			<subrecord rec_id="1" rec_time="5" name="May's father bought her an encyclopaedia. " sound="nj_08a_05.mp3" from="  0:17.013" to="  0:21.153" allWordsNum="6"/>
			<subrecord rec_id="2" rec_time="5" name="Here are three articles she found in it. " sound="nj_08a_05.mp3" from="  0:21.468" to="  0:25.113" allWordsNum="8"/>
			</record> 
			<record rec_id="1" rec_time="25" sound="nj_08a_05.mp3">
			<subrecord rec_id="3" rec_time="8" name="Dinosaurs lived on the Earth over sixty million years ago. This was a long time before people existed. " sound="nj_08a_05.mp3" from="  0:28.353" to="  0:36.273" allWordsNum="18"/>
			<subrecord rec_id="4" rec_time="5" name="Dinosaurs lived everywhere. " sound="nj_08a_05.mp3" from="  0:36.768" to="  0:39.153" allWordsNum="3"/>
			</record>
			<record rec_id="2" rec_time="25" sound="nj_08a_05.mp3">
			<subrecord rec_id="5" rec_time="5" name="Some were as small as chickens. " sound="nj_08a_05.mp3" from="  0:39.513" to="  0:41.988" allWordsNum="6"/>
			<subrecord rec_id="6" rec_time="5" name="Others were as big as ten elephants. " sound="nj_08a_05.mp3" from="  0:42.078" to="  0:45.183" allWordsNum="7"/>
			<subrecord rec_id="7" rec_time="5" name="Some dinosaurs even had wings and could fly. " sound="nj_08a_05.mp3" from="  0:45.273" to="  0:49.143" allWordsNum="8"/>
			</record>
			<record rec_id="3" rec_time="25" sound="nj_08a_05.mp3">
			<subrecord rec_id="8" rec_time="5" name="Many dinosaurs were harmless. " sound="nj_08a_05.mp3" from="  0:49.998" to="  0:52.743" allWordsNum="4"/>
			<subrecord rec_id="9" rec_time="5" name="They were as gentle as sheep and ate plants. " sound="nj_08a_05.mp3" from="  0:52.878" to="  0:56.163" allWordsNum="9"/>
			<subrecord rec_id="10" rec_time="5" name="Others were harmful. " sound="nj_08a_05.mp3" from="  0:56.343" to="  0:58.413" allWordsNum="3"/>
			<subrecord rec_id="11" rec_time="5" name="They were fiercer than tigers and ate meat. " sound="nj_08a_05.mp3" from="  0:58.503" to="  1:01.833" allWordsNum="8"/>
			</record>
			<record rec_id="4" rec_time="25" sound="nj_08a_05.mp3">
			<subrecord rec_id="12" rec_time="5" name="Dinosaurs all died out suddenly. " sound="nj_08a_05.mp3" from="  1:02.868" to="  1:06.018" allWordsNum="5"/>
			<subrecord rec_id="13" rec_time="5" name="Nobody knows why. " sound="nj_08a_05.mp3" from="  1:06.198" to="  1:08.133" allWordsNum="3"/>
			<subrecord rec_id="14" rec_time="12" name="We know about the lives of dinosaurs from the skeletons, eggs and footprints they left behind. (See Earth History)" sound="nj_08a_05.mp3" from="  1:08.493" to="  1:18.888" allWordsNum="19"/>
			</record>
			<record rec_id="5" rec_time="25" sound="nj_08a_05.mp3">
			<subrecord rec_id="15" rec_time="5" name="Diogenes was a famous thinker. " sound="nj_08a_05.mp3" from="  1:23.703" to="  1:26.493" allWordsNum="5"/>
			<subrecord rec_id="16" rec_time="5" name="He lived in Greece about two thousand years ago. " sound="nj_08a_05.mp3" from="  1:26.943" to="  1:30.768" allWordsNum="9"/>
			<subrecord rec_id="17" rec_time="6" name="He taught that the way to be happy was to own as few things as possible. " sound="nj_08a_05.mp3" from="  1:31.398" to="  1:37.068" allWordsNum="16"/>
			</record>
			<record rec_id="6" rec_time="30" sound="nj_08a_05.mp3">
			<subrecord rec_id="18" rec_time="8" name="All he owned was a big jar that he lived in, a coat, a bag and a cup. " sound="nj_08a_05.mp3" from="  1:37.743" to="  1:44.808" allWordsNum="18"/>
			<subrecord rec_id="19" rec_time="3" name="He was very happy. " sound="nj_08a_05.mp3" from="  1:45.033" to="  1:46.788" allWordsNum="4"/>
			<subrecord rec_id="20" rec_time="15" name="One day, Diogenes saw a boy drinking water from his hands by a fountain, so he threw away his cup and became even happier. (See Greece)" sound="nj_08a_05.mp3" from="  1:47.508" to="  1:59.568" allWordsNum="26"/>
			</record>
			<record rec_id="7" rec_time="25" sound="nj_08a_05.mp3">
			<subrecord rec_id="21" rec_time="5" name="Disneyland is a famous amusement park in the USA. " sound="nj_08a_05.mp3" from="  2:03.618" to="  2:08.298" allWordsNum="9"/>
			<subrecord rec_id="22" rec_time="15" name="Disneyland was created by Walt Disney (1901-1966), who is famous for his cartoon characters: Mickey Mouse, Donald Duck, Goofy, Snow White and many others. " sound="nj_08a_05.mp3" from="  2:08.478" to="  2:23.283" allWordsNum="25"/>
			</record>
			<record rec_id="8" rec_time="30" sound="nj_08a_05.mp3">
			<subrecord rec_id="23" rec_time="5" name="Disney was born in the USA. " sound="nj_08a_05.mp3" from="  2:23.733" to="  2:26.613" allWordsNum="6"/>
			<subrecord rec_id="24" rec_time="8" name="After leaving school, he sold newspapers and delivered mail. " sound="nj_08a_05.mp3" from="  2:26.883" to="  2:31.428" allWordsNum="9"/>
			<subrecord rec_id="25" rec_time="5" name="At the same time, he studied art at night. " sound="nj_08a_05.mp3" from="  2:31.653" to="  2:35.343" allWordsNum="9"/>
			<subrecord rec_id="26" rec_time="8" name="Finally he got a job that he really liked——rawing cartoons for films. " sound="nj_08a_05.mp3" from="  2:35.838" to="  2:41.868" allWordsNum="13"/>
			</record>
			<record rec_id="9" rec_time="25" sound="nj_08a_05.mp3">
			<subrecord rec_id="27" rec_time="6" name="Disney's most famous cartoon character, Mickey Mouse, was a real mouse. " sound="nj_08a_05.mp3" from="  2:42.543" to="  2:48.078" allWordsNum="11"/>
			<subrecord rec_id="28" rec_time="6" name="It sometimes sat on Disney's desk while he was working. " sound="nj_08a_05.mp3" from="  2:48.303" to="  2:52.083" allWordsNum="10"/>
			<subrecord rec_id="29" rec_time="5" name="Disney drew the mouse and put it in a cartoon. " sound="nj_08a_05.mp3" from="  2:52.713" to="  2:56.493" allWordsNum="10"/>
			<subrecord rec_id="30" rec_time="8" name="Soon Mickey became a star and Disney became rich and famous. (See Cartoons)" sound="nj_08a_05.mp3" from="  2:56.448" to="  3:04.233" allWordsNum="13"/>
			</record>
			<exercise type="4" index="0"/>
		</part>
	</texts>
