	<texts>
		<part part_index="0" name="Head to head" page="37">
			<record rec_id="0" rec_time="25" sound="nj_09a_03.mp3">
				<subrecord rec_id="1" rec_time="5" name="Emma and Matt give their opinions about pet dogs. " sound="nj_09a_03.mp3" from="  0:00.000" to="  0:03.514" allWordsNum="9"/>
				<subrecord rec_id="2" rec_time="5" name="Is it a good idea to keep pet dogs?" sound="nj_09a_03.mp3" from="  0:03.570" to="  0:06.333" allWordsNum="9"/>
				</record>
			<record rec_id="1" rec_time="25" sound="nj_09a_03.mp3">
				<subrecord rec_id="3" rec_time="5" name="Keeping pet dogs is a good idea, says Emma. " sound="nj_09a_03.mp3" from="  0:06.559" to="  0:10.017" allWordsNum="9"/>
				</record>
			<record rec_id="2" rec_time="25" sound="nj_09a_03.mp3">
				<subrecord rec_id="4" rec_time="8" name="I think that having a pet dog can change a person's life. " sound="nj_09a_03.mp3" from="  0:10.637" to="  0:14.997" allWordsNum="12"/>
				<subrecord rec_id="5" rec_time="6" name="Therefore, people should be allowed to have pet dogs. " sound="nj_09a_03.mp3" from="  0:15.204" to="  0:18.455" allWordsNum="9"/>
				<subrecord rec_id="6" rec_time="3" name="There are lots of reasons for this. " sound="nj_09a_03.mp3" from="  0:18.568" to="  0:20.297" allWordsNum="7"/>
				</record>
			<record rec_id="3" rec_time="25" sound="nj_09a_03.mp3">
				<subrecord rec_id="7" rec_time="5" name="First, dogs are really cute. " sound="nj_09a_03.mp3" from="  0:20.635" to="  0:23.266" allWordsNum="5"/>
				<subrecord rec_id="8" rec_time="12" name="It's nice to pick them up and hold them in our arms, and it's wonderful to see them growing up quickly. " sound="nj_09a_03.mp3" from="  0:23.473" to="  0:29.055" allWordsNum="21"/>
			</record>
			<record rec_id="4" rec_time="60" sound="nj_09a_03.mp3">
				<subrecord rec_id="9" rec_time="8" name="Second, we can learn about responsibility from keeping pet dogs. " sound="nj_09a_03.mp3" from="  0:29.619" to="  0:34.618" allWordsNum="10"/>
				<subrecord rec_id="10" rec_time="6" name="We have to feed them, train them and play with them. " sound="nj_09a_03.mp3" from="  0:34.825" to="  0:38.226" allWordsNum="11"/>
				<subrecord rec_id="11" rec_time="5" name="We even have to take care of them at night. " sound="nj_09a_03.mp3" from="  0:38.358" to="  0:40.763" allWordsNum="10"/>
				<subrecord rec_id="12" rec_time="10" name="According to my mum, this helps us become more responsible people. " sound="nj_09a_03.mp3" from="  0:40.876" to="  0:45.293" allWordsNum="11"/>
			</record>
			<record rec_id="5" rec_time="25" sound="nj_09a_03.mp3">
				<subrecord rec_id="13" rec_time="6" name="We can also learn about life and death from dogs. " sound="nj_09a_03.mp3" from="  0:46.533" to="  0:49.672" allWordsNum="10"/>
				<subrecord rec_id="14" rec_time="12" name="Young people can learn how to care for others and how to respect all living things by keeping a pet dog. " sound="nj_09a_03.mp3" from="  0:49.747" to="  0:56.137" allWordsNum="21"/>
				</record>
			<record rec_id="6" rec_time="25" sound="nj_09a_03.mp3">
				<subrecord rec_id="15" rec_time="6" name="A dog will love you faithfully for many years. " sound="nj_09a_03.mp3" from="  0:57.001" to="  1:00.046" allWordsNum="9"/>
				<subrecord rec_id="16" rec_time="6" name="Keeping pet dogs is something everyone can enjoy. " sound="nj_09a_03.mp3" from="  1:00.347" to="  1:03.729" allWordsNum="8"/>
				</record>
			<record rec_id="7" rec_time="25" sound="nj_09a_03.mp3">
				<subrecord rec_id="17" rec_time="8" name="Keeping pet dogs is not a good idea, says Matt. " sound="nj_09a_03.mp3" from="  1:04.650" to="  1:08.917" allWordsNum="10"/>
				</record>
			<record rec_id="8" rec_time="25" sound="nj_09a_03.mp3">
				<subrecord rec_id="18" rec_time="8" name="In my opinion, it is never a good idea to keep a pet dog. " sound="nj_09a_03.mp3" from="  1:09.593" to="  1:13.634" allWordsNum="14"/>
				</record>
			<record rec_id="9" rec_time="60" sound="nj_09a_03.mp3">
				<subrecord rec_id="19" rec_time="5" name="Pet dogs create a lot of mess. " sound="nj_09a_03.mp3" from="  1:14.216" to="  1:16.641" allWordsNum="7"/> 
				<subrecord rec_id="20" rec_time="14" name="They leave fur and hair on the floor, on beds and on sofas, and they need to be washed regularly. " sound="nj_09a_03.mp3" from="  1:16.735" to="  1:23.989" allWordsNum="20"/>
				<subrecord rec_id="21" rec_time="8" name="Some people are not very good at cleaning up after their dogs when they take them for a walk. " sound="nj_09a_03.mp3" from="  1:24.384" to="  1:29.327" allWordsNum="19"/>
			  <subrecord rec_id="22" rec_time="10" name="As a result, these dogs create a lot of mess on the streets and in parks. " sound="nj_09a_03.mp3" from="  1:29.646" to="  1:34.984" allWordsNum="16"/>
				</record>
			<record rec_id="10" rec_time="60" sound="nj_09a_03.mp3">
				<subrecord rec_id="23" rec_time="6" name="We should remember that not all dogs are friendly. " sound="nj_09a_03.mp3" from="  1:35.604" to="  1:38.742" allWordsNum="9"/>
				<subrecord rec_id="24" rec_time="6" name="Some dogs bark at people they do not know. " sound="nj_09a_03.mp3" from="  1:39.118" to="  1:42.125" allWordsNum="9"/>
				<subrecord rec_id="25" rec_time="5" name="This can frighten young children. " sound="nj_09a_03.mp3" from="  1:42.257" to="  1:44.437" allWordsNum="5"/>
				<subrecord rec_id="26" rec_time="5" name="A few dogs bark all night. " sound="nj_09a_03.mp3" from="  1:44.794" to="  1:47.312" allWordsNum="6"/>
				<subrecord rec_id="27" rec_time="6" name="This stops their neighbours from getting any sleep. " sound="nj_09a_03.mp3" from="  1:47.538" to="  1:50.733" allWordsNum="8"/>
				<subrecord rec_id="28" rec_time="6" name="A small number of pet dogs even bite people. " sound="nj_09a_03.mp3" from="  1:51.034" to="  1:54.567" allWordsNum="9"/>
			</record>
			<record rec_id="11" rec_time="60" sound="nj_09a_03.mp3">
				<subrecord rec_id="29" rec_time="6" name="What's more, most people in modern cities live in flats. " sound="nj_09a_03.mp3" from="  1:55.281" to="  1:58.871" allWordsNum="10"/>
				<subrecord rec_id="30" rec_time="6" name="They have no choice but to keep their dogs in small spaces. " sound="nj_09a_03.mp3" from="  1:58.833" to="  2:02.498" allWordsNum="12"/>
				<subrecord rec_id="31" rec_time="6" name="This may make the dogs feel extremely unhappy. " sound="nj_09a_03.mp3" from="  2:02.893" to="  2:06.332" allWordsNum="8"/>
				<subrecord rec_id="32" rec_time="10" name="They always need fresh air and large open spaces where they can run free. " sound="nj_09a_03.mp3" from="  2:06.614" to="  2:11.914" allWordsNum="14"/>
			</record>
			<record rec_id="12" rec_time="25" sound="nj_09a_03.mp3">
				<subrecord rec_id="33" rec_time="8" name="Finally, paying for dog food and visiting the vet can be expensive. " sound="nj_09a_03.mp3" from="  2:12.797" to="  2:17.552" allWordsNum="12"/>
				<subrecord rec_id="34" rec_time="6" name="I believe that people should not keep pet dogs. " sound="nj_09a_03.mp3" from="  2:18.247" to="  2:21.517" allWordsNum="9"/>
			</record>
			<exercise type="4" index="0"/>
		</part>
			<part part_index="1" name="The Fisherman and the Fish" page="47">
				<record rec_id="0" rec_time="60" sound="nj_09a_03.mp3">
					<subrecord rec_id="1" rec_time="8" name="Once upon a time in Russia, there lived an old fisherman and his wife. " sound="nj_09a_03.mp3" from="  5:36.534" to="  5:41.176" allWordsNum="14"/>
					<subrecord rec_id="2" rec_time="8" name="They were poor and lived in an old, mud hut near the sea. " sound="nj_09a_03.mp3" from="  5:41.247" to="  5:45.696" allWordsNum="13"/>
					<subrecord rec_id="3" rec_time="8" name="One morning, something unusual happened while the fisherman was at work. " sound="nj_09a_03.mp3" from="  5:46.046" to="  5:50.170" allWordsNum="11"/>
					<subrecord rec_id="4" rec_time="6" name="He caught a small, golden fish in his net. " sound="nj_09a_03.mp3" from="  5:50.470" to="  5:53.920" allWordsNum="9"/>
				</record>
				<record rec_id="1" rec_time="25" sound="nj_09a_03.mp3">
					<subrecord rec_id="5" rec_time="12" name="The fish begged the fisherman to let him go and offered to grant the fisherman any wish in return. " sound="nj_09a_03.mp3" from="  5:53.920" to="  6:00.094" allWordsNum="19"/>
					<subrecord rec_id="6" rec_time="10" name="However, the man was kind and asked for nothing before returning the fish to the sea. " sound="nj_09a_03.mp3" from="  6:00.219" to="  6:06.218" allWordsNum="16"/>
					</record>
				<record rec_id="2" rec_time="60" sound="nj_09a_03.mp3">
					<subrecord rec_id="7" rec_time="6" name="The man went home and told his wife about the fish. " sound="nj_09a_03.mp3" from="  6:07.092" to="  6:10.842" allWordsNum="11"/>
					<subrecord rec_id="8" rec_time="8" name="She shouted at him angrily and called him a fool for not making a wish. " sound="nj_09a_03.mp3" from="  6:11.667" to="  6:16.666" allWordsNum="15"/>
					<subrecord rec_id="9" rec_time="14" name="She sent him back to the sea to catch the fish and ask for a new bucket because theirs bucket was broken. " sound="nj_09a_03.mp3" from="  6:16.816" to="  6:24.065" allWordsNum="22"/>
				</record>
				<record rec_id="3" rec_time="60" sound="nj_09a_03.mp3">
					<subrecord rec_id="10" rec_time="10" name="The Sun was shining as the man met the fish again and when he returned, he saw a new bucket. " sound="nj_09a_03.mp3" from="  6:24.290" to="  6:30.289" allWordsNum="20"/>
					<subrecord rec_id="11" rec_time="8" name="But the fisherman's wife was still not happy and wanted a new house. " sound="nj_09a_03.mp3" from="  6:30.839" to="  6:35.613" allWordsNum="13"/>
					<subrecord rec_id="12" rec_time="8" name="The weather was getting bad as the man explained to the fish about his rude wife. " sound="nj_09a_03.mp3" from="  6:36.038" to="  6:40.887" allWordsNum="16"/>
					<subrecord rec_id="13" rec_time="8" name="The fish comforted him and promised to grant the second wish. " sound="nj_09a_03.mp3" from="  6:41.312" to="  6:45.361" allWordsNum="11"/>
					<subrecord rec_id="14" rec_time="6" name="When he returned, he saw a nice, new cottage. " sound="nj_09a_03.mp3" from="  6:45.536" to="  6:49.535" allWordsNum="9"/>
					</record>
				<record rec_id="4" rec_time="60" sound="nj_09a_03.mp3">
					<subrecord rec_id="15" rec_time="8" name="The next day, his wife said that she wanted to be mayor of the city. " sound="nj_09a_03.mp3" from="  6:50.685" to="  6:55.609" allWordsNum="15"/>
					<subrecord rec_id="16" rec_time="12" name="The waves were getting big and the sky was becoming dark as the man apologized to the golden fish. " sound="nj_09a_03.mp3" from="  6:55.934" to="  7:01.958" allWordsNum="19"/>
					<subrecord rec_id="17" rec_time="8" name="Again, like the wishes before, this wish was granted. " sound="nj_09a_03.mp3" from="  7:02.308" to="  7:07.107" allWordsNum="9"/>
					<subrecord rec_id="18" rec_time="10" name="When he returned, he found his wife dressed in expensive clothes and giving orders to servants. " sound="nj_09a_03.mp3" from="  7:07.332" to="  7:13.206" allWordsNum="16"/>\
					<subrecord rec_id="19" rec_time="12" name="She told her husband that he still had to sleep in the hut even though she now had a bigger house. " sound="nj_09a_03.mp3" from="  7:13.731" to="  7:20.080" allWordsNum="21"/>
					<subrecord rec_id="20" rec_time="10" name="She was a very greedy woman and the following week she demanded to be queen of all the land. " sound="nj_09a_03.mp3" from="  7:20.530" to="  7:25.929" allWordsNum="19"/>
				</record>
				<record rec_id="5" rec_time="60" sound="nj_09a_03.mp3">
					<subrecord rec_id="21" rec_time="6" name="Again the fisherman went down to the shore. " sound="nj_09a_03.mp3" from="  7:26.879" to="  7:30.029" allWordsNum="8"/>
					<subrecord rec_id="22" rec_time="5" name="By now there was a terrible storm. " sound="nj_09a_03.mp3" from="  7:30.254" to="  7:32.878" allWordsNum="7"/>
					<subrecord rec_id="23" rec_time="8" name="There was thunder and lightning, and giant waves in the sea. " sound="nj_09a_03.mp3" from="  7:33.028" to="  7:37.277" allWordsNum="11"/>
					<subrecord rec_id="24" rec_time="10" name="The man shouted as loudly as he could so that the fish could hear him. " sound="nj_09a_03.mp3" from="  7:37.552" to="  7:42.577" allWordsNum="15"/>
				</record>
				<record rec_id="6" rec_time="60" sound="nj_09a_03.mp3">
					<subrecord rec_id="25" rec_time="10" name="This time the fish did not reply and turned around to swim out to sea. " sound="nj_09a_03.mp3" from="  7:42.727" to="  7:47.926" allWordsNum="15"/>
					<subrecord rec_id="26" rec_time="8" name="The fisherman waited but the golden fish never came back. " sound="nj_09a_03.mp3" from="  7:48.201" to="  7:52.525" allWordsNum="10"/>
					<subrecord rec_id="27" rec_time="12" name="When the man returned home, he found his wife in the old, mud hut holding their broken bucket. " sound="nj_09a_03.mp3" from="  7:52.750" to="  7:59.499" allWordsNum="18"/>
				</record>
			<exercise type="4" index="0"/>
		</part>
