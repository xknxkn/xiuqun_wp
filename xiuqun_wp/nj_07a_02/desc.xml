	<texts>
		<part part_index="0" name="Read" page="12">
			<record rec_id="0" rec_time="25" sound="nj_07a_02.mp3">
			<subrecord rec_id="1" rec_time="5" name="Every year, the SPCA saves many animals in Garden City." sound="nj_07a_02.mp3" from="1:27.937" to="1:33.166" allWordsNum="10"/>
			<subrecord rec_id="2" rec_time="5" name="Sometimes people are unkind to their pets." sound="nj_07a_02.mp3" from="1:33.724" to="1:37.374" allWordsNum="7"/>
			<subrecord rec_id="3" rec_time="5" name="They do not look after their pets and leave them in the street." sound="nj_07a_02.mp3" from="1:37.692" to="1:41.555" allWordsNum="13"/>
			<subrecord rec_id="4" rec_time="8" name="SPCA officers save animals from danger and take them to the SPCA." sound="nj_07a_02.mp3" from="1:42.509" to="1:48.560" allWordsNum="12"/>
		  </record>
			<record rec_id="1" rec_time="25" sound="nj_07a_02.mp3">
			<subrecord rec_id="5" rec_time="5" name="The SPCA also helps animals find new homes." sound="nj_07a_02.mp3" from="1:49.685" to="1:53.717" allWordsNum="8"/>
			<subrecord rec_id="6" rec_time="5" name="If people want a pet, they can go to the SPCA." sound="nj_07a_02.mp3" from="1:54.190" to="1:58.291" allWordsNum="11"/>
			<subrecord rec_id="7" rec_time="5" name="However, they must promise to take care of their new pet." sound="nj_07a_02.mp3" from="1:58.833" to="2:02.931" allWordsNum="11"/>
			</record>
			<record rec_id="2" rec_time="20" sound="nj_07a_02.mp3">
			<subrecord rec_id="8" rec_time="5" name="There are clinics for sick animals in the SPCA." sound="nj_07a_02.mp3" from="2:04.107" to="2:07.777" allWordsNum="9"/>
			<subrecord rec_id="9" rec_time="5" name="The vets in the clinics take care of them." sound="nj_07a_02.mp3" from="2:08.205" to="2:11.127" allWordsNum="9"/>
			</record>
			<exercise type="4" index="0"/>
		</part>
	</texts>
	<dialogs>
		<part part_index="0" name="Listen and act" page="10">
			<!--<record rec_id="0" rec_time="5" name="Mr Hu and the children are visiting the SPCA." sound="nj_07a_02.mp3" from="0:14.561" to="0:18.745" allWordsNum="9"/>-->
			<record rec_id="0" rec_time="12" name="Good morning, children. Welcome to the SPCA. I'm an SPCA officer. Do you know what the SPCA is?" sound="nj_07a_02.mp3" from="0:21.776" to="0:32.000" allWordsNum="18"/>
			<record rec_id="1" rec_time="5" name="It is the Society for the Prevention of Cruelty to Animals." sound="nj_07a_02.mp3" from="0:32.851" to="0:37.621" allWordsNum="11"/>
			<record rec_id="2" rec_time="5" name="That's right!" sound="nj_07a_02.mp3" from="0:38.221" to="0:39.653" allWordsNum="2"/>
			<record rec_id="3" rec_time="14" name="Someone left these puppies and their mother in the street. They had no food or water. They were hungry and thirsty. I found them and took them to the SPCA." sound="nj_07a_02.mp3" from="0:43.525" to="0:55.963" allWordsNum="30"/>
			<record rec_id="4" rec_time="10" name="Look, children. The puppies are lovely. Would you like to keep one as your pet? Which one do you like best?" sound="nj_07a_02.mp3" from="0:59.370" to="1:08.316" allWordsNum="21"/>
			<record rec_id="5" rec_time="5" name="I prefer the black one." sound="nj_07a_02.mp3" from="1:08.537" to="1:10.528" allWordsNum="5"/>
			<record rec_id="6" rec_time="5" name="I prefer the yellow and brown one." sound="nj_07a_02.mp3" from="1:11.061" to="1:14.229" allWordsNum="7"/>
			<record rec_id="7" rec_time="5" name="I prefer the white one." sound="nj_07a_02.mp3" from="1:14.817" to="1:16.920" allWordsNum="5"/>
			<exercise type="5" index="0"/>
		</part>
    </dialogs>
