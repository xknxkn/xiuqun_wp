	<texts>
		<part part_index="0" name="Read a story" page="70">      
			<record rec_id="0" rec_time="30" sound="nj_06a_10.mp3">
				<subrecord rec_id="1" rec_time="3" name="Mr Lin lived in the city. " sound="nj_06a_10.mp3" from="1:21.014" to="1:23.981" allWordsNum="6"/>
				<subrecord rec_id="2" rec_time="2" name="He was fat and unhealthy. " sound="nj_06a_10.mp3" from="1:24.448" to="1:26.602" allWordsNum="5"/>
			  <subrecord rec_id="3" rec_time="5" name="He had an unhealthy diet and he did no exercise. " sound="nj_06a_10.mp3" from="1:27.293" to="1:31.947" allWordsNum="10"/>
			  <subrecord rec_id="4" rec_time="3" name="Miss Chen was his cousin. " sound="nj_06a_10.mp3" from="1:32.577" to="1:34.934" allWordsNum="5"/>
			  <subrecord rec_id="5" rec_time="2" name="She Lived in the countryside. " sound="nj_06a_10.mp3" from="1:35.503" to="1:37.474" allWordsNum="5"/>
			  <subrecord rec_id="6" rec_time="2" name="She was fit and healthy. " sound="nj_06a_10.mp3" from="1:38.002" to="1:39.994" allWordsNum="5"/>
			  <subrecord rec_id="7" rec_time="5" name="She had a healthy diet and worked in her garden every day. " sound="nj_06a_10.mp3" from="1:40.726" to="1:45.460" allWordsNum="12"/>
			</record>
			<record rec_id="1" rec_time="10" sound="nj_06a_10.mp3">
				<subrecord rec_id="8" rec_time="5" name="One day, Mr Lin went to visit Miss Chen and had lunch with her. " sound="nj_06a_10.mp3" from="1:46.416" to="1:51.496" allWordsNum="14"/>
				<subrecord rec_id="9" rec_time="4" name="'What would you like for lunch?' asked Miss Chen. " sound="nj_06a_10.mp3" from="1:52.329" to="1:55.987" allWordsNum="9"/>
	   	</record>
		  <record rec_id="2" rec_time="6" sound="nj_06a_10.mp3">
				<subrecord rec_id="10" rec_time="6" name="'I'd like some pizza, some chips and some ice cream,' said Mr Lin. " sound="nj_06a_10.mp3" from="1:56.698" to="2:02.470" allWordsNum="13"/>
			</record>
		  <record rec_id="3" rec_time="20" sound="nj_06a_10.mp3">
				<subrecord rec_id="11" rec_time="3" name="'I don't have that kind of food in my house. " sound="nj_06a_10.mp3" from="2:03.425" to="2:06.371" allWordsNum="10"/>
				<subrecord rec_id="12" rec_time="2" name="I eat a lot of rice. " sound="nj_06a_10.mp3" from="2:06.981" to="2:08.952" allWordsNum="6"/>
				<subrecord rec_id="13" rec_time="3" name="I eat plenty of fresh fruit and vegetables. " sound="nj_06a_10.mp3" from="2:09.379" to="2:12.305" allWordsNum="8"/>
				<subrecord rec_id="14" rec_time="8" name="I also eat some meat and fish, but I only eat a little salt and sugar,' said Miss Chen. " sound="nj_06a_10.mp3" from="2:13.017" to="2:21.023" allWordsNum="19"/>
			</record>
		  <record rec_id="4" rec_time="20" sound="nj_06a_10.mp3">
				<subrecord rec_id="15" rec_time="4" name="Mr Lin stayed with his cousin for a few weeks. " sound="nj_06a_10.mp3" from="2:21.958" to="2:25.352" allWordsNum="10"/>
				<subrecord rec_id="16" rec_time="2" name="He worked in Miss Chen's garden. " sound="nj_06a_10.mp3" from="2:26.022" to="2:27.994" allWordsNum="6"/>
				<subrecord rec_id="17" rec_time="7" name="He ate a lot of rice, plenty of fresh fruit and vegetables, and some meat and fish. " sound="nj_06a_10.mp3" from="2:28.644" to="2:34.964" allWordsNum="17"/>
				<subrecord rec_id="18" rec_time="3" name="He became fit and healthy. " sound="nj_06a_10.mp3" from="2:35.695" to="2:38.195" allWordsNum="5"/>
			</record>
				<exercise type="4" index="0"/>
		</part>
	</texts>
	<dialogs>
		<part part_index="0" name="Good diets and bad diets" page="69">
			<record rec_id="0" rec_time="4" name="How much fruit do we need every day?" sound="nj_06a_10.mp3" from="0:59.034" to="1:02.692" allWordsNum="8"/>
			<record rec_id="1" rec_time="4" name="We need plenty of fruit every day. " sound="nj_06a_10.mp3" from="1:02.793" to="1:06.532" allWordsNum="7"/>
			<record rec_id="2" rec_time="4" name="How much sugar do we need every day?" sound="nj_06a_10.mp3" from="1:09.004" to="1:12.824" allWordsNum="8"/>
			<record rec_id="3" rec_time="3" name="We need a little sugar every day. " sound="nj_06a_10.mp3" from="1:12.824" to="1:16.035" allWordsNum="7"/>
			<exercise type="5" index="0"/>
		</part>
		<part part_index="1" name="Read" page="71">
			<record rec_id="0" rec_time="4" name="Joe, what did you have for breakfast yesterday?" sound="nj_06a_10.mp3" from="2:44.495" to="2:48.493" allWordsNum="8"/>
			<record rec_id="1" rec_time="8" name="I had eggs and milk. That's my favourite breakfast. What did you have for breakfast, Alice?" sound="nj_06a_10.mp3" from="2:48.590" to="2:56.251" allWordsNum="16"/>
			<record rec_id="2" rec_time="5" name="I had some porridge. What did you have for lunch?" sound="nj_06a_10.mp3" from="2:56.394" to="3:01.007" allWordsNum="10"/>
			<record rec_id="3" rec_time="9" name="I had three hamburgers, a lot of chips and a lot of cola for lunch. What did you have for lunch?" sound="nj_06a_10.mp3" from="3:01.047" to="3:09.521" allWordsNum="21"/>
			<record rec_id="4" rec_time="10" name="I had steamed chicken with rice and some vegetable soup. I also had a little tea. What did you have for dinner?" sound="nj_06a_10.mp3" from="3:09.643" to="3:20.007" allWordsNum="22"/>
			<record rec_id="5" rec_time="8" name="I had pizza and chips. Then I had a lot of ice cream. What did you have for dinner?" sound="nj_06a_10.mp3" from="3:20.211" to="3:27.994" allWordsNum="19"/>
			<record rec_id="6" rec_time="11" name="I had some steamed fish, some rice and a lot of boiled vegetables for dinner. Joe, your diet is unhealthy. " sound="nj_06a_10.mp3" from="3:28.095" to="3:39.110" allWordsNum="20"/>
			<exercise type="5" index="0"/>
		</part>
		<part part_index="2" name="Eating habits" page="73">
			<record rec_id="0" rec_time="10" name="Alice, here's a quiz about eating habits. Let's do it together. Do you drink a lot of water every day?" sound="nj_06a_10.mp3" from="4:30.970" to="4:40.989" allWordsNum="20"/>
			<record rec_id="1" rec_time="3" name="No, I don't. " sound="nj_06a_10.mp3" from="4:41.070" to="4:43.976" allWordsNum="3"/>
			<record rec_id="2" rec_time="3" name="Do you eat a lot of spicy food?" sound="nj_06a_10.mp3" from="4:44.159" to="4:47.451" allWordsNum="8"/>
			<record rec_id="3" rec_time="3" name="Yes, I do. " sound="nj_06a_10.mp3" from="4:47.593" to="4:50.012" allWordsNum="3"/>
			<exercise type="5" index="0"/>
		</part>
