	<dialogs>
		<part part_index="0" name="Listen and say" page="2">
			<record rec_id="0" rec_time="15" name="Look, Ben, I've got a letter from Uncle Weiming. He lives in Beijing. He has invited us to stay with his family in August." sound="nj_07a_01.mp3" from="0:18.164" to="0:28.555" allWordsNum="24"/>
			<record rec_id="1" rec_time="6" name="I haven't seen my cousins before." sound="nj_07a_01.mp3" from="0:28.750" to="0:32.514" allWordsNum="6"/>
			<record rec_id="2" rec_time="10" name="Your uncle has sent us a photograph. These are your cousins, Simon and Lucy." sound="nj_07a_01.mp3" from="0:32.501" to="0:39.649" allWordsNum="14"/>
			<record rec_id="3" rec_time="6" name="How old are they?" sound="nj_07a_01.mp3" from="0:39.884" to="0:42.384" allWordsNum="4"/>
			<record rec_id="4" rec_time="6" name="Simon is twelve and Lucy is ten. " sound="nj_07a_01.mp3" from="0:42.696" to="0:46.707" allWordsNum="7"/>
			<record rec_id="5" rec_time="5" name="Are we going to travel to Beijing by plane?" sound="nj_07a_01.mp3" from="0:47.176" to="0:50.639" allWordsNum="9"/>
			<record rec_id="6" rec_time="8" name="Perhaps, but plane tickets are very expensive." sound="nj_07a_01.mp3" from="0:50.822" to="0:55.496" allWordsNum="7"/>
			<record rec_id="7" rec_time="8" name="Let's go by train. It's cheaper and more interesting." sound="nj_07a_01.mp3" from="0:55.600" to="1:00.978" allWordsNum="9"/>
			<record rec_id="8" rec_time="12" name="That's a good idea. Let's to talk to Mum and Kitty.  I'm going to get some brochures from the travel agent." sound="nj_07a_01.mp3" from="1:01.173" to="1:10.262" allWordsNum="21"/>
			<exercise type="5" index="0"/>
		</part>
		<part part_index="1" name="At the travel agent's" page="3">
			<record rec_id="0" rec_time="5" name="Good morning. Can I help you?" sound="nj_07a_01.mp3" from="1:47.568" to="1:50.433" allWordsNum="6"/>
			<record rec_id="1" rec_time="15" name="Yes, we'd like to travel to Beijing by plane. How long does it take to travel from Garden City to Beijing by plane?" sound="nj_07a_01.mp3" from="1:50.915" to="2:00.720" allWordsNum="23"/>
			<record rec_id="2" rec_time="5" name="It takes about two and a half hours." sound="nj_07a_01.mp3" from="2:01.750" to="2:04.600" allWordsNum="8"/>
			<record rec_id="3" rec_time="5" name="How much does it cost?" sound="nj_07a_01.mp3" from="2:05.500" to="2:07.634" allWordsNum="5"/>
			<record rec_id="4" rec_time="5" name="One thousand and five hundred yuan." sound="nj_07a_01.mp3" from="2:08.055" to="2:10.473" allWordsNum="6"/>
			<record rec_id="5" rec_time="5" name="That's expensive." sound="nj_07a_01.mp3" from="2:11.550" to="2:13.090" allWordsNum="2"/>
			<exercise type="5" index="0"/>
		</part>
		<part part_index="2" name="Listen and say" page="4">
		  <record rec_id="0" rec_time="5" name="When are we going to visit Beijing?" sound="nj_07a_01.mp3" from="2:21.586" to="2:24.502" allWordsNum="7"/>
			<record rec_id="1" rec_time="5" name="We're going to visit Beijing on 16 August." sound="nj_07a_01.mp3" from="2:25.313" to="2:29.347" allWordsNum="9"/>
			<record rec_id="2" rec_time="5" name="How long are we going to stay there?" sound="nj_07a_01.mp3" from="2:30.199" to="2:32.663" allWordsNum="8"/>
			<record rec_id="3" rec_time="10" name="We're going to stay there for two weeks. We're going to come back at the end of August." sound="nj_07a_01.mp3" from="2:33.245" to="2:39.913" allWordsNum="17"/>
			<record rec_id="4" rec_time="5" name="How are we going to get there?" sound="nj_07a_01.mp3" from="2:43.307" to="2:45.687" allWordsNum="6"/>
			<record rec_id="5" rec_time="5" name="We're going to get there by plane." sound="nj_07a_01.mp3" from="2:46.598" to="2:48.717" allWordsNum="8"/>
			<record rec_id="6" rec_time="5" name="Can we travel by train? It's cheaper." sound="nj_07a_01.mp3" from="2:49.769" to="2:53.130" allWordsNum="7"/> 
			<record rec_id="7" rec_time="10" name="The train is slower than the plane. It takes fifteen hours to travel from Garden City to Beijing." sound="nj_07a_01.mp3" from="2:53.999" to="3:01.401" allWordsNum="18"/>
			<exercise type="5" index="0"/>
