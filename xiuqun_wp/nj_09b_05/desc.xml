	<texts>
		<part part_index="0" name="Tom Sawyer paints the fence" page="73">
			<record rec_id="0" rec_time="20" sound="nj_09b_05.mp3">
			<subrecord rec_id="1" rec_time="6" name="Saturday morning came, and everything was bright and fresh. " sound="nj_09b_05.mp3" from="  0:27.466" to="  0:32.729" allWordsNum="9"/>
			<subrecord rec_id="2" rec_time="8" name="Every boy in town was happy, except Tom Sawyer. " sound="nj_09b_05.mp3" from="  0:32.729" to="  0:37.678" allWordsNum="9"/>
			</record>
			<record rec_id="1" rec_time="60" sound="nj_09b_05.mp3">
			<subrecord rec_id="3" rec_time="8" name="Tom stood on the sidewalk with a bucket of white paint and a brush with a long handle. " sound="nj_09b_05.mp3" from="  0:37.914" to="  0:44.591" allWordsNum="18"/>
			<subrecord rec_id="4" rec_time="6" name="He looked at the fence that his aunt had told him to paint, and felt very depressed. " sound="nj_09b_05.mp3" from="  0:45.141" to="  0:50.404" allWordsNum="17"/>
			<subrecord rec_id="5" rec_time="8" name="It was thirty yards long, and three yards high! He dipped his brush in the paint and painted a corner of the fence. " sound="nj_09b_05.mp3" from="  0:50.976" to="  0:58.065" allWordsNum="23"/>
			<subrecord rec_id="6" rec_time="5" name="Then he stood back and looked at it. " sound="nj_09b_05.mp3" from="  0:59.338" to="  1:01.390" allWordsNum="8"/>
			<subrecord rec_id="7" rec_time="6" name="The section he had painted was so small, and the unpainted section was so huge! " sound="nj_09b_05.mp3" from="  1:02.374" to="  1:08.177" allWordsNum="15"/>
			<subrecord rec_id="8" rec_time="5" name="He sat down under a tree to have a rest. " sound="nj_09b_05.mp3" from="  1:09.213" to="  1:11.942" allWordsNum="10"/>
			</record>
			<record rec_id="2" rec_time="25" sound="nj_09b_05.mp3">
			<subrecord rec_id="9" rec_time="5" name="He began to think of the games which he had planned for this day. " sound="nj_09b_05.mp3" from="  1:12.782" to="  1:15.164" allWordsNum="14"/>
			<subrecord rec_id="10" rec_time="5" name="Soon the boys who were free would come along and make fun of him. " sound="nj_09b_05.mp3" from="  1:15.917" to="  1:19.573" allWordsNum="14"/>
			<subrecord rec_id="11" rec_time="6" name="But just then, he had a wonderful idea. He picked up his brush and went back to work. " sound="nj_09b_05.mp3" from="  1:20.219" to="  1:26.194" allWordsNum="18"/>
			</record>
			<record rec_id="3" rec_time="25" sound="nj_09b_05.mp3">
			<subrecord rec_id="12" rec_time="5" name="Ben Rogers came along the road, singing happily and carrying an apple. " sound="nj_09b_05.mp3" from="  1:28.337" to="  1:33.173" allWordsNum="12"/>
			<subrecord rec_id="13" rec_time="5" name="He stopped and looked at Tom. " sound="nj_09b_05.mp3" from="  1:34.278" to="  1:36.419" allWordsNum="6"/>
			<subrecord rec_id="14" rec_time="5" name="Tom went on painting and ignored Ben. " sound="nj_09b_05.mp3" from="  1:37.145" to="  1:39.770" allWordsNum="7"/>			
			</record>
			<record rec_id="4" rec_time="25" sound="nj_09b_05.mp3">
			<subrecord rec_id="15" rec_time="3" name="I'm going swimming, said Ben. " sound="nj_09b_05.mp3" from="  1:41.013" to="  1:43.155" allWordsNum="9"/>
			<subrecord rec_id="16" rec_time="3" name="Do you want to go swimming, Tom?" sound="nj_09b_05.mp3" from="  1:43.638" to="  1:45.538" allWordsNum="8"/>
			<subrecord rec_id="17" rec_time="3" name="But I guess you can't. " sound="nj_09b_05.mp3" from="  1:46.022" to="  1:47.541" allWordsNum="7"/>
			<subrecord rec_id="18" rec_time="3" name="You have to stay and work, don't you?" sound="nj_09b_05.mp3" from="  1:47.714" to="  1:50.201" allWordsNum="11"/>
			</record>
			<record rec_id="5" rec_time="60" sound="nj_09b_05.mp3">
			<subrecord rec_id="19" rec_time="3" name="Tom went on painting. " sound="nj_09b_05.mp3" from="  1:52.032" to="  1:53.551" allWordsNum="4"/>
			<subrecord rec_id="20" rec_time="3" name="Work? he said. " sound="nj_09b_05.mp3" from="  1:54.000" to="  1:55.451" allWordsNum="5"/>
			<subrecord rec_id="21" rec_time="3" name="This isn't work. " sound="nj_09b_05.mp3" from="  1:56.142" to="  1:57.385" allWordsNum="6"/>
			<subrecord rec_id="22" rec_time="3" name="I'm enjoying myself. " sound="nj_09b_05.mp3" from="  1:58.111" to="  1:59.665" allWordsNum="5"/>
			<subrecord rec_id="23" rec_time="5" name="Does a boy get a chance to paint a fence like this every day?" sound="nj_09b_05.mp3" from="  2:00.390" to="  2:03.775" allWordsNum="15"/>
			</record>
			<record rec_id="6" rec_time="60" sound="nj_09b_05.mp3">			
			<subrecord rec_id="24" rec_time="4" name="Ben thought about this. " sound="nj_09b_05.mp3" from="  2:05.261" to="  2:06.608" allWordsNum="4"/>
			<subrecord rec_id="25" rec_time="4" name="Tom went on painting, carefully. " sound="nj_09b_05.mp3" from="  2:07.609" to="  2:10.165" allWordsNum="5"/>
			<subrecord rec_id="26" rec_time="8" name="Sometimes he stopped and stood back to look at the fence, and then added a bit more paint in just the right place. " sound="nj_09b_05.mp3" from="  2:11.443" to="  2:18.697" allWordsNum="23"/>
			<subrecord rec_id="27" rec_time="4" name="Ben was watching every move. " sound="nj_09b_05.mp3" from="  2:19.526" to="  2:21.702" allWordsNum="5"/>
			<subrecord rec_id="28" rec_time="5" name="He was getting more and more interested. " sound="nj_09b_05.mp3" from="  2:22.669" to="  2:24.949" allWordsNum="7"/>
			</record>
			<record rec_id="7" rec_time="5" sound="nj_09b_05.mp3">				
			<subrecord rec_id="29" rec_time="5" name="After a while, he said, Tom, will you let me do some painting?" sound="nj_09b_05.mp3" from="  2:26.123" to="  2:31.304" allWordsNum="15"/>
			</record>
			<record rec_id="8" rec_time="25" sound="nj_09b_05.mp3">				
			<subrecord rec_id="30" rec_time="5" name="Tom thought about it, but said, No, Ben, I can't. " sound="nj_09b_05.mp3" from="  2:32.444" to="  2:36.554" allWordsNum="10"/>
			<subrecord rec_id="31" rec_time="5" name="You see, Aunt Polly wants is to be done properly. " sound="nj_09b_05.mp3" from="  2:36.900" to="  2:41.044" allWordsNum="10"/>
		  <subrecord rec_id="32" rec_time="5" name="I'm the only one who can do it right. " sound="nj_09b_05.mp3" from="  2:41.528" to="  2:43.911" allWordsNum="12"/>
			</record>
			<record rec_id="9" rec_time="60" sound="nj_09b_05.mp3">			
			<subrecord rec_id="33" rec_time="3" name="Oh, please, Tom, begged Ben. " sound="nj_09b_05.mp3" from="  2:45.293" to="  2:47.814" allWordsNum="5"/>
			<subrecord rec_id="34" rec_time="3" name="I can do it. I'll be really careful. " sound="nj_09b_05.mp3" from="  2:48.263" to="  2:51.372" allWordsNum="11"/>
			<subrecord rec_id="35" rec_time="5" name="Just let me try. I'll give you half of my apple. " sound="nj_09b_05.mp3" from="  2:51.372" to="  2:55.482" allWordsNum="13"/>
			<subrecord rec_id="36" rec_time="3" name="I'll give you all of it!" sound="nj_09b_05.mp3" from="  2:55.793" to="  2:57.140" allWordsNum="9"/>
			</record>
			<record rec_id="10" rec_time="5" sound="nj_09b_05.mp3">			
			<subrecord rec_id="37" rec_time="5" name="Well, all right, Ben, said Tom. But you must be careful. " sound="nj_09b_05.mp3" from="  2:58.280" to="  3:02.977" allWordsNum="15"/>
			</record>
			<record rec_id="11" rec_time="20" sound="nj_09b_05.mp3">			
			<subrecord rec_id="38" rec_time="6" name="He gave Ben his brush with worry on his face but joy in his heart. " sound="nj_09b_05.mp3" from="  3:04.325" to="  3:08.849" allWordsNum="15"/>
			<subrecord rec_id="39" rec_time="6" name="He sat down again under the tree, and started to eat the apple that Ben had given him. " sound="nj_09b_05.mp3" from="  3:09.817" to="  3:14.479" allWordsNum="18"/>
			</record>
		  <record rec_id="12" rec_time="60" sound="nj_09b_05.mp3">			
			<subrecord rec_id="40" rec_time="5" name="All day, boys came to make fun, but they stayed to paint. " sound="nj_09b_05.mp3" from="  3:15.585" to="  3:20.213" allWordsNum="12"/>
			<subrecord rec_id="41" rec_time="5" name="When Ben got tired, Billy Fisher was waiting. " sound="nj_09b_05.mp3" from="  3:20.697" to="  3:24.047" allWordsNum="8"/>
			<subrecord rec_id="42" rec_time="5" name="He gave Tom a kite for a chance to paint. " sound="nj_09b_05.mp3" from="  3:24.600" to="  3:27.363" allWordsNum="10"/>									
			<subrecord rec_id="43" rec_time="5" name="Then Johnny Miller offered him twelve marbles, and so on. " sound="nj_09b_05.mp3" from="  3:27.985" to="  3:31.715" allWordsNum="10"/>
		  </record>
		  <record rec_id="13" rec_time="60" sound="nj_09b_05.mp3">
			<subrecord rec_id="44" rec_time="8" name="By the afternoon Tom had gained all sorts of toys, and the fence had gained three coats of paint. " sound="nj_09b_05.mp3" from="  3:32.371" to="  3:38.692" allWordsNum="19"/>
			<subrecord rec_id="45" rec_time="5" name="Later Aunt Polly came to look at Tom's work. " sound="nj_09b_05.mp3" from="  3:39.245" to="  3:42.319" allWordsNum="11"/>									
			<subrecord rec_id="46" rec_time="6" name="She was so pleased when she saw the painted fence that she gave Tom a large cake!" sound="nj_09b_05.mp3" from="  3:42.492" to="  3:48.053" allWordsNum="17"/>
   	  </record>
			<exercise type="4" index="0"/>
		</part>
			<part part_index="1" name="An extract from The Adventures of Tom Sawyer" page="83">
			<record rec_id="0" rec_time="60" sound="nj_09b_05.mp3">
      <subrecord rec_id="1" rec_time="8" name="Tom and Becky walked down a winding passage inside a cave, holding their candles high. " sound="nj_09b_05.mp3" from="  8:45.977" to="  8:53.003" allWordsNum="15"/>
			<subrecord rec_id="2" rec_time="8" name="They saw names and dates written on the wall with candle smoke by other visitors to the cave. " sound="nj_09b_05.mp3" from="  8:53.126" to="  8:59.969" allWordsNum="18"/>
			<subrecord rec_id="3" rec_time="6" name="Becky and Tom read some of them as they walked along. " sound="nj_09b_05.mp3" from="  9:00.580" to="  9:04.918" allWordsNum="11"/>
			</record>
			<record rec_id="1" rec_time="30" sound="nj_09b_05.mp3">
      <subrecord rec_id="4" rec_time="13" name="They were talking so much that it was a little while before they noticed they were now in a part of the cave that had no writing on the wall. " sound="nj_09b_05.mp3" from="  9:05.284" to="  9:14.938" allWordsNum="30"/>				
			<subrecord rec_id="5" rec_time="6" name="They used candle smoke to write their own names on the rock and walked on. " sound="nj_09b_05.mp3" from="  9:15.366" to="  9:20.803" allWordsNum="15"/>
			</record>
			<record rec_id="2" rec_time="60" sound="nj_09b_05.mp3">
			<subrecord rec_id="6" rec_time="5" name="Soon they came to a place where a little stream of water was running. " sound="nj_09b_05.mp3" from="  9:21.231" to="  9:26.119" allWordsNum="14"/>
			<subrecord rec_id="7" rec_time="8" name=" Over the years the cave wall had been shaped by the running stream, and it looked like a frozen waterfall. " sound="nj_09b_05.mp3" from="  9:26.608" to="  9:34.367" allWordsNum="20"/>
			<subrecord rec_id="8" rec_time="8" name="Tom went behind it and lit it up with his candle so that Becky could see it better. " sound="nj_09b_05.mp3" from="  9:34.856" to="  9:41.393" allWordsNum="18"/>
			<subrecord rec_id="9" rec_time="6" name="Behind the stone waterfall he found an opening in the rock, leading downwards. " sound="nj_09b_05.mp3" from="  9:42.127" to="  9:48.114" allWordsNum="13"/>
      <subrecord rec_id="10" rec_time="3" name="At once he wanted to explore it. " sound="nj_09b_05.mp3" from="  9:48.298" to="  9:50.986" allWordsNum="7"/>			
			</record>
			<record rec_id="3" rec_time="60" sound="nj_09b_05.mp3">
			<subrecord rec_id="11" rec_time="7" name="Becky agreed and together they started to walk down into the earth. " sound="nj_09b_05.mp3" from="  9:50.986" to="  9:56.790" allWordsNum="12"/>
			<subrecord rec_id="12" rec_time="10" name="They wound this way and that, far into the cave, making smoke marks here and there to show the way back. " sound="nj_09b_05.mp3" from="  9:57.218" to=" 10:05.589" allWordsNum="21"/>
			<subrecord rec_id="13" rec_time="3" name="This was exciting. " sound="nj_09b_05.mp3" from=" 10:05.650" to=" 10:07.421" allWordsNum="3"/>
			<subrecord rec_id="14" rec_time="6" name="They would have a lot to tell their friends above when they returned. " sound="nj_09b_05.mp3" from=" 10:07.605" to=" 10:12.248" allWordsNum="13"/>
			</record>
			<record rec_id="4" rec_time="60" sound="nj_09b_05.mp3">
			<subrecord rec_id="15" rec_time="10" name="In one place, they found a huge, open space full of shining stalactites, as long and thick as a man's leg. " sound="nj_09b_05.mp3" from=" 10:12.187" to=" 10:20.863" allWordsNum="23"/>
			<subrecord rec_id="16" rec_time="10" name="They walked around it and left by one of the many passages that opened into it. " sound="nj_09b_05.mp3" from=" 10:21.108" to=" 10:26.545" allWordsNum="16"/>
			<subrecord rec_id="17" rec_time="5" name="The next place they found was full of bats. " sound="nj_09b_05.mp3" from=" 10:27.156" to=" 10:30.211" allWordsNum="9"/>
			<subrecord rec_id="18" rec_time="3" name="There were thousands of them. " sound="nj_09b_05.mp3" from=" 10:30.272" to=" 10:32.166" allWordsNum="5"/>
