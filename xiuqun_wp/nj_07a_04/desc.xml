			<subrecord rec_id="13" rec_time="8" name="He works for a removal company. " sound="nj_07a_04.mp3" from="  0:52.100" to="  0:54.596" allWordsNum="6"/>	
			<subrecord rec_id="14" rec_time="8" name="He moves people's furniture to their new flats." sound="nj_07a_04.mp3" from="  0:54.719" to="  0:58.115" allWordsNum="8"/>
			</record>
