	<dialogs>
		<part part_index="0" name="say and act" page="29">
			<record rec_id="0" rec_time="5" name="Good morning, Susan." sound="nj_07a_04.mp3" from="2:27.920" to="2:30.208" allWordsNum="3"/>
			<record rec_id="1" rec_time="6" name="Good morning, Kitty. Are you going to school?" sound="nj_07a_04.mp3" from="  2:29.993" to="  2:33.781" allWordsNum="8"/>
			<record rec_id="2" rec_time="6" name="Yes, I am. Are you going to work?" sound="nj_07a_04.mp3" from="  2:33.781" to="  2:37.140" allWordsNum="8"/>
			<record rec_id="3" rec_time="3" name="Yes, I am." sound="nj_07a_04.mp3" from="  2:37.212" to="  2:38.998" allWordsNum="3"/>
			<record rec_id="4" rec_time="3" name="What's your job?" sound="nj_07a_04.mp3" from="  2:39.427" to="  2:41.285" allWordsNum="3"/>
			<record rec_id="5" rec_time="5" name="I'm a secretary." sound="nj_07a_04.mp3" from="  2:41.214" to="  2:43.430" allWordsNum="3"/>
			<record rec_id="6" rec_time="5" name="Where do you work?" sound="nj_07a_04.mp3" from="  2:43.501" to="  2:45.717" allWordsNum="4"/>
			<record rec_id="7" rec_time="6" name="I work in an office in the city centre." sound="nj_07a_04.mp3" from="  2:45.574" to="  2:49.147" allWordsNum="9"/>
			<record rec_id="8" rec_time="5" name="What do you do there?" sound="nj_07a_04.mp3" from="   2:49.433" to="  2:51.935" allWordsNum="5"/>
			<record rec_id="9" rec_time="16" name="I answer the phone. I type letters. I go to meetings with my manager and take notes. I do many different things." sound="nj_07a_04.mp3" from="  2:52.006" to="  3:00.940" allWordsNum="22"/>
			<record rec_id="10" rec_time="5" name="Do you wear a uniform at work?" sound="nj_07a_04.mp3" from="  3:01.154" to="  3:04.442" allWordsNum="7"/>
			<record rec_id="11" rec_time="3" name="No, I don't." sound="nj_07a_04.mp3" from="  3:04.156" to="  3:06.300" allWordsNum="3"/>
			<record rec_id="12" rec_time="3" name="Do you like your job?" sound="nj_07a_04.mp3" from="  3:06.229" to="  3:08.659" allWordsNum="5"/>
			<record rec_id="13" rec_time="12" name="Yes, I do. It's very interesting and I enjoy working with all the people in my office." sound="nj_07a_04.mp3" from="  3:08.873" to="  3:15.448" allWordsNum="17"/>
			<exercise type="5" index="0"/>
		</part>
	</dialogs>
