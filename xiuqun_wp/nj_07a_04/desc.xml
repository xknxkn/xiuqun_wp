		<part part_index="1" name="read" page="32">
			<record rec_id="0" rec_time="25" sound="nj_07a_04.mp3">
			<subrecord rec_id="1" rec_time="10" name="Every morning, Tom sees a lot of people on his way to school." sound="nj_07a_04.mp3" from="  4:59.506" to="  5:04.576" allWordsNum="13"/>
			</record>
			<record rec_id="2" rec_time="25" sound="nj_07a_04.mp3">
			<subrecord rec_id="2" rec_time="6" name="He sees the street cleaners cleaning the streets." sound="nj_07a_04.mp3" from="  5:04.655" to="  5:08.497" allWordsNum="8"/>
			<subrecord rec_id="3" rec_time="5" name="They start work early." sound="nj_07a_04.mp3" from="  5:08.338" to="  5:10.596" allWordsNum="4"/>
			<subrecord rec_id="4" rec_time="6" name="They sweep the streets and collect rubbish. " sound="nj_07a_04.mp3" from="  5:10.477" to="  5:13.962" allWordsNum="7"/>			
			<subrecord rec_id="5" rec_time="6" name="They also empty the rubbish bins." sound="nj_07a_04.mp3" from="  5:13.962" to="  5:17.210" allWordsNum="6"/>
			</record>
			<record rec_id="3" rec_time="30" sound="nj_07a_04.mp3">
			<subrecord rec_id="6" rec_time="5" name="Mr Ma has a bakery." sound="nj_07a_04.mp3" from="  5:17.329" to="  5:20.022" allWordsNum="5"/>
			<subrecord rec_id="7" rec_time="5" name="He bakes bread and cakes." sound="nj_07a_04.mp3" from="  5:19.982" to="  5:22.675" allWordsNum="5"/>
			<subrecord rec_id="8" rec_time="6" name="He opens his shop early." sound="nj_07a_04.mp3" from="  5:22.755" to="  5:25.804" allWordsNum="5"/>
			<subrecord rec_id="9" rec_time="10" name="Tom sees people going to his shop to buy bread and cakes." sound="nj_07a_04.mp3" from="  5:25.725" to="  5:31.230" allWordsNum="12"/>		
			</record>
			<record rec_id="4" rec_time="20" sound="nj_07a_04.mp3">
			<subrecord rec_id="10" rec_time="6" name="Mrs Liang sells newspapers." sound="nj_07a_04.mp3" from="  5:31.547" to="  5:34.874" allWordsNum="4"/>
			<subrecord rec_id="11" rec_time="6" name="She starts work early, too." sound="nj_07a_04.mp3" from="  5:34.953" to="  5:37.844" allWordsNum="5"/>
			</record>
			<record rec_id="5" rec_time="30" sound="nj_07a_04.mp3">
			<subrecord rec_id="12" rec_time="8" name="Tom sees people buying newspapers from her." sound="nj_07a_04.mp3" from="  5:37.924" to="  5:41.924" allWordsNum="7"/>
			<subrecord rec_id="13" rec_time="10" name="Tom sees Mrs Hu selling flowers in her flower shop." sound="nj_07a_04.mp3" from="  5:41.963" to="  5:47.033" allWordsNum="10"/>
			<subrecord rec_id="14" rec_time="12" name="She goes to the flower market early every morning to buy flowers for her shop." sound="nj_07a_04.mp3" from="  5:47.072" to="  5:53.964" allWordsNum="15"/>
			</record>
			<exercise type="4" index="0"/>
			</part>
