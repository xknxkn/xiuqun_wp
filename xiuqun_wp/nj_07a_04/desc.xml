			<record rec_id="0" rec_time="25" sound="nj_07a_04.mp3">
			<subrecord rec_id="1" rec_time="14" name="Mrs Wang, Miss Tang, Susan and David live in the same block of flats as Ben and Kitty." sound="nj_07a_04.mp3" from="  0:16.992" to="  0:24.603" allWordsNum="18"/>
			<subrecord rec_id="2" rec_time="5" name="They have different jobs." sound="nj_07a_04.mp3" from="0:24.439" to="0:27.058" allWordsNum="4"/>
			</record>
			<record rec_id="1" rec_time="25" sound="nj_07a_04.mp3">	
			<subrecord rec_id="3" rec_time="5" name="Mrs Wang is a doctor." sound="nj_07a_04.mp3" from=" 0:27.017" to=" 0:29.349" allWordsNum="5"/>
			<subrecord rec_id="4" rec_time="5" name="She works in a hospital. " sound="nj_07a_04.mp3" from="  0:29.186" to="  0:31.150" allWordsNum="5"/>
			<subrecord rec_id="5" rec_time="5" name="She makes sick people better." sound="nj_07a_04.mp3" from="  0:31.150" to="  0:33.973" allWordsNum="5"/>
			</record>
			<record rec_id="2" rec_time="25" sound="nj_07a_04.mp3">	
			<subrecord rec_id="6" rec_time="5" name="Miss Tang is an architect." sound="nj_07a_04.mp3" from="  0:34.137" to="  0:36.715" allWordsNum="5"/>
			<subrecord rec_id="7" rec_time="8" name="She works for a construction company." sound="nj_07a_04.mp3" from="  0:36.633" to="  0:39.579" allWordsNum="6"/>
			<subrecord rec_id="8" rec_time="5" name="She draws plans of buildings." sound="nj_07a_04.mp3" from="  0:39.333" to="  0:42.034" allWordsNum="5"/>
			</record>
			<record rec_id="3" rec_time="25" sound="nj_07a_04.mp3">	
