			<subrecord rec_id="10" rec_time="5" name="She works in an office." sound="nj_07a_04.mp3" from="  0:44.857" to="  0:46.699" allWordsNum="5"/>
			<subrecord rec_id="11" rec_time="5" name="She types letters." sound="nj_07a_04.mp3" from="  0:46.740  " to="0:48.745" allWordsNum="3"/>
			</record>
			<record rec_id="4" rec_time="20" sound="nj_07a_04.mp3">	
