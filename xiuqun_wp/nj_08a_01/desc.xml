	<texts>
		<part part_index="0" name="Reading" page="3">
			<record rec_id="0" rec_time="40" sound="nj_08a_01.mp3">
			<subrecord rec_id="1" rec_time="20" name="2 Middle Street, Walker, Newcastle, NE 6 4 BY, England, UK, 30 August, Dear May." sound="nj_08a_01.mp3" from="0:10.034" to="0:24.912" allWordsNum="13"/>
			<subrecord rec_id="2" rec_time="10" name="I saw your name in Penfriends magazine, and I would like to be your penfriend." sound="nj_08a_01.mp3" from="0:24.945" to="0:30.797" allWordsNum="15"/>
			<subrecord rec_id="3" rec_time="5" name="First, I will tell you something about myself." sound="nj_08a_01.mp3" from="0:30.812" to="0:34.375" allWordsNum="8"/>
			<subrecord rec_id="4" rec_time="5" name="My name is Jonathan Chen." sound="nj_08a_01.mp3" from="0:34.359" to="0:36.857" allWordsNum="5"/>
			</record>
			<record rec_id="1" rec_time="30" sound="nj_08a_01.mp3">
			<subrecord rec_id="5" rec_time="5" name="You can call me Jon. " sound="nj_08a_01.mp3" from="0:36.874" to="0:38.819" allWordsNum="5"/>
			<subrecord rec_id="6" rec_time="10" name="I am twelve years old, and I am about one and a half metres tall." sound="nj_08a_01.mp3" from="0:38.854" to="0:43.956" allWordsNum="15"/>
			<subrecord rec_id="7" rec_time="5" name="I have very short black hair." sound="nj_08a_01.mp3" from="0:43.952" to="0:46.503" allWordsNum="6"/>
			<subrecord rec_id="8" rec_time="5" name="My hobby is playing chess." sound="nj_08a_01.mp3" from="0:46.488" to="0:49.484" allWordsNum="5"/>
			<subrecord rec_id="9" rec_time="5" name="I live with my parents in the UK." sound="nj_08a_01.mp3" from="0:49.973" to="0:53.725" allWordsNum="8"/>
			</record>
			<record rec_id="2" rec_time="35" sound="nj_08a_01.mp3">
			<subrecord rec_id="10" rec_time="5" name="They are from Shanghai." sound="nj_08a_01.mp3" from="0:53.740" to="0:55.742" allWordsNum="4"/>
			<subrecord rec_id="11" rec_time="5" name="We live in a small house." sound="nj_08a_01.mp3" from="0:55.742" to="0:58.397" allWordsNum="6"/>
			<subrecord rec_id="12" rec_time="10" name="My dad owns a Chinese restaurant nearby and my mum works in a college." sound="nj_08a_01.mp3" from="0:58.397" to="1:05.293" allWordsNum="14"/>
			<subrecord rec_id="13" rec_time="5" name="I have a brother." sound="nj_08a_01.mp3" from="1:05.322" to="1:07.072" allWordsNum="4"/>
			<subrecord rec_id="14" rec_time="5" name="His name is Willie." sound="nj_08a_01.mp3" from="1:07.117" to="1:09.045" allWordsNum="4"/>
			<subrecord rec_id="15" rec_time="5" name="He is at university in London." sound="nj_08a_01.mp3" from="1:08.822" to="1:11.862" allWordsNum="6"/>
			</record>
			<record rec_id="3" rec_time="30" sound="nj_08a_01.mp3">
			<subrecord rec_id="16" rec_time="5" name="He is studying to be an accountant." sound="nj_08a_01.mp3" from="1:11.788" to="1:14.917" allWordsNum="7"/>
			<subrecord rec_id="17" rec_time="5" name="He comes back home during the holidays." sound="nj_08a_01.mp3" from="1:14.932" to="1:18.506" allWordsNum="7"/>
			<subrecord rec_id="18" rec_time="5" name="How many people are there in your family?" sound="nj_08a_01.mp3" from="1:18.388" to="1:21.443" allWordsNum="8"/>
			<subrecord rec_id="19" rec_time="3" name="What do they do?" sound="nj_08a_01.mp3" from="1:21.428" to="1:23.281" allWordsNum="4"/>
			<subrecord rec_id="20" rec_time="8" name="I am in Year 7 at Walker School." sound="nj_08a_01.mp3" from="1:23.385" to="1:27.226" allWordsNum="8"/>
			</record>
			<record rec_id="4" rec_time="35" sound="nj_08a_01.mp3">
			<subrecord rec_id="21" rec_time="8" name="It is near my house, and I can walk to school." sound="nj_08a_01.mp3" from="1:27.241" to="1:31.023" allWordsNum="11"/>
			<subrecord rec_id="22" rec_time="8" name="I like my school because the teachers are very friendly." sound="nj_08a_01.mp3" from="1:30.978" to="1:35.561" allWordsNum="10"/>
			<subrecord rec_id="23" rec_time="5" name="I am keen on sports." sound="nj_08a_01.mp3" from="1:35.590" to="1:37.934" allWordsNum="5"/>
			<subrecord rec_id="24" rec_time="6" name="I enjoy skiing and playing table tennis." sound="nj_08a_01.mp3" from="1:37.978" to="1:41.671" allWordsNum="7"/>
			<subrecord rec_id="25" rec_time="5" name="My best subject is Physics." sound="nj_08a_01.mp3" from="1:41.715" to="1:44.577" allWordsNum="5"/>
			</record>
			<record rec_id="5" rec_time="40" sound="nj_08a_01.mp3">
			<subrecord rec_id="26" rec_time="10" name="My ambition is to be an architect, or maybe an engineer." sound="nj_08a_01.mp3" from="1:44.592" to="1:49.990" allWordsNum="11"/>
			<subrecord rec_id="27" rec_time="10" name="I enclosed a photo of myself and some of my school friends." sound="nj_08a_01.mp3" from="1:50.035" to="1:55.478" allWordsNum="12"/>
			<subrecord rec_id="28" rec_time="5" name="I am in the middle." sound="nj_08a_01.mp3" from="1:55.507" to="1:57.420" allWordsNum="5"/>
			<subrecord rec_id="29" rec_time="6" name="I also enclosed a photo of our school." sound="nj_08a_01.mp3" from="1:57.465" to="2:01.232" allWordsNum="8"/>
			<subrecord rec_id="30" rec_time="5" name="It is beautiful, isn't it?" sound="nj_08a_01.mp3" from="2:01.291" to="2:03.619" allWordsNum="5"/>
			</record>
			<record rec_id="6" rec_time="20" sound="nj_08a_01.mp3">
			<subrecord rec_id="31" rec_time="10" name="I hope you will write to me soon and tell me all about yourself." sound="nj_08a_01.mp3" from="2:03.664" to="2:08.676" allWordsNum="14"/>
			<subrecord rec_id="32" rec_time="3" name="Yours sincerely." sound="nj_08a_01.mp3" from="2:08.721" to="2:10.382" allWordsNum="2"/>
			<subrecord rec_id="33" rec_time="3" name="Jon." sound="nj_08a_01.mp3" from="2:10.441" to="2:11.657" allWordsNum="1"/>
		  </record>
			<exercise type="4" index="0"/>
