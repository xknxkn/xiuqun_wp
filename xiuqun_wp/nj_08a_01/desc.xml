		<part part_index="1" name="Anna's Blog" page="14">
			<record rec_id="0" rec_time="25" sound="nj_08a_01.mp3">
			<subrecord rec_id="1" rec_time="2" name="Hello." sound="nj_08a_01.mp3" from="4:36.086" to="4:37.218" allWordsNum="1"/>
			<subrecord rec_id="2" rec_time="3" name="Welcome to my blog." sound="nj_08a_01.mp3" from="4:37.236" to="4:39.089" allWordsNum="4"/>
			<subrecord rec_id="3" rec_time="3" name="My name is Anna." sound="nj_08a_01.mp3" from="4:39.054" to="4:41.062" allWordsNum="4"/>
			<subrecord rec_id="4" rec_time="4" name="I am a girl from Germany." sound="nj_08a_01.mp3" from="4:41.131" to="4:43.825" allWordsNum="6"/>
			<subrecord rec_id="5" rec_time="4" name="I am 13 years old." sound="nj_08a_01.mp3" from="4:43.859" to="4:46.039" allWordsNum="5"/>
			<subrecord rec_id="6" rec_time="8" name="I am tall and thin, and I have long, blonde hair." sound="nj_08a_01.mp3" from="4:46.210" to="4:50.895" allWordsNum="11"/>
			 </record>
			<record rec_id="2" rec_time="45" sound="nj_08a_01.mp3">                            
			<subrecord rec_id="7" rec_time="4" name="I like many sports." sound="nj_08a_01.mp3" from="4:51.770" to="4:53.967" allWordsNum="4"/>
			<subrecord rec_id="8" rec_time="4" name="I love playing badminton." sound="nj_08a_01.mp3" from="4:54.001" to="4:56.043" allWordsNum="4"/>
			<subrecord rec_id="9" rec_time="6" name="I am keen on swimming and playing tennis too." sound="nj_08a_01.mp3" from="4:56.198" to="4:59.544" allWordsNum="9"/>
			<subrecord rec_id="10" rec_time="8" name="I also like listening to pop music and drawing pictures." sound="nj_08a_01.mp3" from="4:59.784" to="5:04.332" allWordsNum="10"/>
			<subrecord rec_id="11" rec_time="8" name="I have an old guitar and I am learning to play it." sound="nj_08a_01.mp3" from="5:04.640" to="5:08.347" allWordsNum="12"/>
			<subrecord rec_id="12" rec_time="12" name="My native language is German, but I also enjoy reading and speaking English." sound="nj_08a_01.mp3" from="5:08.399" to="5:14.645" allWordsNum="13"/>
			</record>
			<record rec_id="3" rec_time="35" sound="nj_08a_01.mp3">
			<subrecord rec_id="13" rec_time="8" name="My mum is a teacher and my dad is a taxi driver." sound="nj_08a_01.mp3" from="5:15.520" to="5:19.965" allWordsNum="12"/>
			<subrecord rec_id="14" rec_time="5" name="I have a big family." sound="nj_08a_01.mp3" from="5:19.999" to="5:22.058" allWordsNum="5"/>
			<subrecord rec_id="15" rec_time="5" name="We live in a flat." sound="nj_08a_01.mp3" from="5:22.350" to="5:24.769" allWordsNum="5"/>
			<subrecord rec_id="16" rec_time="10" name="The flat is quite small but I like it because it is very close to the mountains!" sound="nj_08a_01.mp3" from="5:25.130" to="5:30.930" allWordsNum="17"/>
			<subrecord rec_id="17" rec_time="6" name="I love drawing pictures of the mountains." sound="nj_08a_01.mp3" from="5:31.136" to="5:34.328" allWordsNum="7"/>
			</record>
			<record rec_id="4" rec_time="30" sound="nj_08a_01.mp3">
			<subrecord rec_id="18" rec_time="6" name="I share a room with my sister, Marina." sound="nj_08a_01.mp3" from="5:34.877" to="5:38.000" allWordsNum="8"/>
			<subrecord rec_id="19" rec_time="3" name="She is 15." sound="nj_08a_01.mp3" from="5:38.052" to="5:39.768" allWordsNum="3"/>
			<subrecord rec_id="20" rec_time="6" name="I have two other little sisters and one brother." sound="nj_08a_01.mp3" from="5:40.025" to="5:44.023" allWordsNum="9"/>
			<subrecord rec_id="21" rec_time="5" name="We all love to have to have family parties together." sound="nj_08a_01.mp3" from="5:44.092" to="5:46.992" allWordsNum="10"/>
			</record>
			<record rec_id="5" rec_time="25" sound="nj_08a_01.mp3">
			<subrecord rec_id="22" rec_time="6" name="I go to a big school by bus from my home." sound="nj_08a_01.mp3" from="5:48.365" to="5:51.986" allWordsNum="11"/>
			<subrecord rec_id="23" rec_time="8" name="My favourite subjects are Maths, Art and English." sound="nj_08a_01.mp3" from="5:52.071" to="5:56.859" allWordsNum="8"/>
			<subrecord rec_id="24" rec_time="8" name="I attend school from 8 a.m. to 1 p.m. on weekdays." sound="nj_08a_01.mp3" from="5:57.305" to="6:01.510" allWordsNum="11"/>
			</record>
			<record rec_id="6" rec_time="25" sound="nj_08a_01.mp3">
			<subrecord rec_id="25" rec_time="5" name="I do not wear a school uniform." sound="nj_08a_01.mp3" from="6:01.595" to="6:03.981" allWordsNum="7"/>
			<subrecord rec_id="26" rec_time="8" name="I like my school because the teachers are all very friendly." sound="nj_08a_01.mp3" from="6:04.667" to="6:08.974" allWordsNum="11"/>
			<subrecord rec_id="27" rec_time="8" name="My ambition is to be an architect when I leave school." sound="nj_08a_01.mp3" from="6:09.438" to="6:13.539" allWordsNum="11"/>
			</record>
			<record rec_id="7" rec_time="25" sound="nj_08a_01.mp3">
			<subrecord rec_id="28" rec_time="6" name="I would like to have penfriends from all over the world!" sound="nj_08a_01.mp3" from="6:14.998" to="6:18.893" allWordsNum="11"/>
			<subrecord rec_id="29" rec_time="10" name="I hope that you will e-mail me, and maybe come and visit me in Germany one day!" sound="nj_08a_01.mp3" from="6:19.116" to="6:24.779" allWordsNum="17"/>
			</record>
