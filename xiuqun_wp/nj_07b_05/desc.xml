		</record>
				<record rec_id="2" rec_time="28" sound="nj_07b_05.mp3">
			<subrecord rec_id="10" rec_time="5" name="'Although we're poor, we live happily,' said Doris. " sound="nj_07b_05.mp3" from="  0:53.083" to="  0:58.111" allWordsNum="10"/>
