			<subrecord rec_id="14" rec_time="5" name="I've lived here with Doris for many years,' replied Fred. " sound="nj_07b_05.mp3" from="  1:13.896" to="  1:17.605" allWordsNum="12"/>
		</record>
				<record rec_id="3" rec_time="20" sound="nj_07b_05.mp3">
