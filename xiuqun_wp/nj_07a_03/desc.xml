		<part part_index="0" name="List and say" page="17">
			<record rec_id="0" rec_time="15" name="Nowadays we can visit countries near or far away from China. We can also read about them in magazines and newspapers. Which country have you visited or read about?" sound="nj_07a_03.mp3" from="0:49.682" to="1:02.892" allWordsNum="29"/>
			<record rec_id="1" rec_time="5" name="I've visited Japan. It's near China." sound="nj_07a_03.mp3" from="1:02.897" to="1:06.829" allWordsNum="6"/>
			<record rec_id="2" rec_time="5" name="I've visited the UK. It's far away from China." sound="nj_07a_03.mp3" from="1:07.182" to="1:11.984" allWordsNum="9"/>
			<record rec_id="3" rec_time="5" name="I've read about Canada. It's far away from China." sound="nj_07a_03.mp3" from="1:11.991" to="1:16.758" allWordsNum="9"/>
			<record rec_id="4" rec_time="5" name="I've read about India. It's near China." sound="nj_07a_03.mp3" from="1:16.758" to="1:21.266" allWordsNum="7"/>
