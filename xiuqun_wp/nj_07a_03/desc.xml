		  <record rec_id="3" rec_time="25" sound="nj_07a_03.mp3">
			<subrecord rec_id="5" rec_time="5" name="I am a girl from Garden City." sound="nj_07a_03.mp3" from="2:33.364" to="2:36.163" allWordsNum="7"/>
			<subrecord rec_id="6" rec_time="5" name="I am Chinese." sound="nj_07a_03.mp3" from="2:36.164" to="2:38.045" allWordsNum="3"/>
			<subrecord rec_id="7" rec_time="5" name="I am twelve years old." sound="nj_07a_03.mp3" from="2:38.068" to="2:40.133" allWordsNum="5"/>
			<subrecord rec_id="8" rec_time="5" name="There are four people in my family." sound="nj_07a_03.mp3" from="2:40.202" to="2:43.208" allWordsNum="7"/>
			<subrecord rec_id="9" rec_time="10" name="They are my mother, my father, my brother, Ben, and me." sound="nj_07a_03.mp3" from="2:43.208" to="2:48.440" allWordsNum="11"/>
			<subrecord rec_id="10" rec_time="5" name="We have a pet dog." sound="nj_07a_03.mp3" from="2:48.486" to="2:50.298" allWordsNum="5"/>
