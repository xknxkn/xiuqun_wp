			</record>
		  <record rec_id="4" rec_time="25" sound="nj_07a_03.mp3">				
			<subrecord rec_id="12" rec_time="5" name="I go to Rose Garden School." sound="nj_07a_03.mp3" from="2:52.823" to="2:55.897" allWordsNum="6"/>
			<subrecord rec_id="13" rec_time="10" name="I am in Grade 7.My favourite subjects are Maths and Music." sound="nj_07a_03.mp3" from="2:55.920" to="3:01.955" allWordsNum="11"/>
			<subrecord rec_id="14" rec_time="10" name="I like riding my bicycle, playing badminton and watching TV." sound="nj_07a_03.mp3" from="3:01.978" to="3:08.059" allWordsNum="10"/>
