	<sentences>
		<part part_index="0" name="Look and read" page="16">
			<record rec_id="0" rec_time="5" name="Garden City is a crowded city." sound="nj_07a_03.mp3" from="0:00.000" to="0:03.380" allWordsNum="6"/>
			<record rec_id="1" rec_time="5" name="Over six million people live in Garden City." sound="nj_07a_03.mp3" from="0:03.380" to="0:07.090" allWordsNum="8"/>
			<record rec_id="2" rec_time="5" name="Most of them are Chinese." sound="nj_07a_03.mp3" from="0:07.090" to="0:09.963" allWordsNum="5"/>
			<record rec_id="3" rec_time="5" name="Many people from other countries live in Garden City, too." sound="nj_07a_03.mp3" from="0:09.963" to="0:14.300" allWordsNum="10"/>
			<record rec_id="4" rec_time="8" name="For example, there are more than twenty-eight thousand people from Canada in Garden City." sound="nj_07a_03.mp3" from="0:14.300" to="0:20.943" allWordsNum="14"/>
			<record rec_id="5" rec_time="12" name="There are also people from the USA, Australia, Japan, the UK and India in Garden City." sound="nj_07a_03.mp3" from="0:20.943" to="0:30.307" allWordsNum="16"/>
			<record rec_id="6" rec_time="5" name="What do we call people from Canada?" sound="nj_07a_03.mp3" from="0:31.474" to="0:34.992" allWordsNum="7"/>
			<record rec_id="7" rec_time="5" name="We call them Canadians." sound="nj_07a_03.mp3" from="0:35.195" to="0:37.766" allWordsNum="4"/>
			<exercise type="3" index="0"/>
		</part>
	</sentences>
