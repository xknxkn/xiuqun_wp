	<texts>
		<part part_index="0" name="Escaping from Gork" page="111">
			<record rec_id="0" rec_time="25" sound="nj_08a_07.mp3">
				<subrecord rec_id="1" rec_time="8" name="After his supper, Gork lay down on his bed and fell asleep almost immediately. " sound="nj_08a_07.mp3" from="  0:24.013" to="  0:30.396" allWordsNum="14"/>
				<subrecord rec_id="2" rec_time="3" name="He started to snore. " sound="nj_08a_07.mp3" from="  0:30.611" to="  0:32.486" allWordsNum="4"/>
				<subrecord rec_id="3" rec_time="3" name="The noise shook the cave. " sound="nj_08a_07.mp3" from="  0:32.690" to="  0:34.912" allWordsNum="5"/>
			</record>
			<record rec_id="1" rec_time="25" sound="nj_08a_07.mp3">
			<subrecord rec_id="4" rec_time="3" name="'Listen, everyone,' I said. " sound="nj_08a_07.mp3" from="  0:35.442" to="  0:37.828" allWordsNum="4"/>
			<subrecord rec_id="5" rec_time="3" name="'Here's my plan.' " sound="nj_08a_07.mp3" from="  0:38.072" to="  0:39.703" allWordsNum="3"/>
			</record>
			<record rec_id="2" rec_time="30" sound="nj_08a_07.mp3">
				<subrecord rec_id="6" rec_time="8" name="From my pocket, I took my small, powerful laser torch. " sound="nj_08a_07.mp3" from="  0:40.009" to="  0:45.106" allWordsNum="10"/>
				<subrecord rec_id="7" rec_time="8" name="Because it was in a plastic case, the magnet could not attract it. " sound="nj_08a_07.mp3" from="  0:45.351" to="  0:50.040" allWordsNum="13"/>
				<subrecord rec_id="8" rec_time="6" name="'We'll use this to escape from the cage,' I said. " sound="nj_08a_07.mp3" from="  0:50.366" to="  0:53.934" allWordsNum="10"/>
				<subrecord rec_id="9" rec_time="6" name="'First, I'll melt the bars and then ...' " sound="nj_08a_07.mp3" from="  0:54.199" to="  0:57.338" allWordsNum="7"/>
			</record>
			<record rec_id="3" rec_time="25" sound="nj_08a_07.mp3">
				<subrecord rec_id="10" rec_time="3" name="Peters interrupted me. " sound="nj_08a_07.mp3" from="  0:57.705" to="  0:59.316" allWordsNum="3"/>
				<subrecord rec_id="11" rec_time="3" name="'I know, Captain. " sound="nj_08a_07.mp3" from="  0:59.377" to="  1:00.661" allWordsNum="3"/>
				<subrecord rec_id="12" rec_time="5" name="Then you'll use the torch to kill Gork.' " sound="nj_08a_07.mp3" from="  1:00.825" to="  1:03.903" allWordsNum="8"/>
			</record>
			<record rec_id="4" rec_time="25" sound="nj_08a_07.mp3">
				<subrecord rec_id="13" rec_time="3" name="'use your brain,' I said. " sound="nj_08a_07.mp3" from="  1:04.311" to="  1:06.778" allWordsNum="5"/>
				<subrecord rec_id="14" rec_time="6" name="'If we kill Gork, how will we escape?" sound="nj_08a_07.mp3" from="  1:06.900" to="  1:10.101" allWordsNum="8"/> 
				<subrecord rec_id="15" rec_time="4" name="We're too weak to open the door. " sound="nj_08a_07.mp3" from="  1:10.162" to="  1:12.506" allWordsNum="7"/>
				<subrecord rec_id="16" rec_time="4" name="Here's what we'll do! We'll ...' " sound="nj_08a_07.mp3" from="  1:12.894" to="  1:15.463" allWordsNum="5"/>
			</record>
			<record rec_id="5" rec_time="35" sound="nj_08a_07.mp3">
				<subrecord rec_id="17" rec_time="6" name="Moments later, we were all out of the cage. " sound="nj_08a_07.mp3" from="  1:16.217" to="  1:19.907" allWordsNum="9"/>
				<subrecord rec_id="18" rec_time="8" name="I went over to the bed, and stood beside Gork's head. " sound="nj_08a_07.mp3" from="  1:19.948" to="  1:24.005" allWordsNum="11"/> 
				<subrecord rec_id="19" rec_time="6" name="I said loudly, 'Gork, this is Nobody.' " sound="nj_08a_07.mp3" from="  1:24.209" to="  1:28.205" allWordsNum="7"/>
				<subrecord rec_id="20" rec_time="3" name="Gork woke up. " sound="nj_08a_07.mp3" from="  1:28.510" to="  1:29.938" allWordsNum="3"/>
				<subrecord rec_id="21" rec_time="6" name="I aimed the torch at his eye and pressed the button. " sound="nj_08a_07.mp3" from="  1:30.264" to="  1:33.954" allWordsNum="11"/>
			  <subrecord rec_id="22" rec_time="4" name="The laser beam hit Gork in the eye. " sound="nj_08a_07.mp3" from="  1:34.178" to="  1:37.032" allWordsNum="8"/>
			</record>
			<record rec_id="6" rec_time="25" sound="nj_08a_07.mp3">
				<subrecord rec_id="23" rec_time="8" name="Gork roared, 'I can't see! My eye!'" sound="nj_08a_07.mp3" from="  1:37.440" to="  1:42.007" allWordsNum="7"/>
				<subrecord rec_id="24" rec_time="6" name="The noise brought the kangaroos running into the room. " sound="nj_08a_07.mp3" from="  1:42.292" to="  1:45.676" allWordsNum="9"/>
			</record>
			<record rec_id="7" rec_time="25" sound="nj_08a_07.mp3">
				<subrecord rec_id="25" rec_time="4" name="'What's wrong, Gork?' they shouted. " sound="nj_08a_07.mp3" from="  1:46.105" to="  1:48.408" allWordsNum="5"/>
			</record>
			<record rec_id="8" rec_time="25" sound="nj_08a_07.mp3">
				<subrecord rec_id="26" rec_time="4" name="Gork shouted back, 'It's Nobody. " sound="nj_08a_07.mp3" from="  1:48.775" to="  1:51.793" allWordsNum="5"/>
				<subrecord rec_id="27" rec_time="3" name="Nobody attacked me. " sound="nj_08a_07.mp3" from="  1:51.793" to="1:53.628" allWordsNum="3"/>
				<subrecord rec_id="28" rec_time="3" name="Nobody damaged my eye.' " sound="nj_08a_07.mp3" from="  1:53.689" to="  1:56.074" allWordsNum="4"/>
			</record>
			<record rec_id="9" rec_time="25" sound="nj_08a_07.mp3">
				<subrecord rec_id="29" rec_time="8" name="The kangaroos laughed and said, 'Nobody attacked him. " sound="nj_08a_07.mp3" from="  1:56.278" to="  2:00.906" allWordsNum="8"/>
				<subrecord rec_id="30" rec_time="4" name="Gork's having a bad dream. " sound="nj_08a_07.mp3" from="  2:00.987" to="  2:03.332" allWordsNum="5"/>				
				<subrecord rec_id="31" rec_time="3" name="Let's go back to bed. " sound="nj_08a_07.mp3" from="  2:03.291" to="  2:04.800" allWordsNum="5"/>
				<subrecord rec_id="32" rec_time="3" name="Good night, Gork.' " sound="nj_08a_07.mp3" from="  2:04.820" to="  2:06.064" allWordsNum="3"/>
			</record>
			<record rec_id="10" rec_time="25" sound="nj_08a_07.mp3">
				<subrecord rec_id="33" rec_time="6" name="My crew and I quickly ran among the kangaroos. " sound="nj_08a_07.mp3" from="  2:08.734" to="  2:12.098" allWordsNum="9"/>
				<subrecord rec_id="34" rec_time="8" name="Gork said, 'Nobody, I know you're hiding among my kangaroos. " sound="nj_08a_07.mp3" from="  2:12.527" to="  2:17.827" allWordsNum="10"/>
				<subrecord rec_id="35" rec_time="6" name="I'll get you. You're finished, all of you.' " sound="nj_08a_07.mp3" from="  2:17.827" to="  2:20.906" allWordsNum="8"/>
			</record>
			<record rec_id="11" rec_time="25" sound="nj_08a_07.mp3">
				<subrecord rec_id="36" rec_time="8" name="He opened the cave door a little and told his kangaroos to go out one by one. " sound="nj_08a_07.mp3" from="  2:21.232" to="  2:26.920" allWordsNum="17"/>
				<subrecord rec_id="37" rec_time="10" name="As they hopped through the door, he felt their backs with his hands to make sure we were not riding on them. " sound="nj_08a_07.mp3" from="  2:27.205" to="  2:33.994" allWordsNum="22"/>
			</record>
			<record rec_id="12" rec_time="25" sound="nj_08a_07.mp3">
				<subrecord rec_id="38" rec_time="5" name="'What do we do now?' Peters whispered. " sound="nj_08a_07.mp3" from="  2:34.586" to="  2:37.440" allWordsNum="7"/>
				<subrecord rec_id="39" rec_time="5" name="'We can't get out on the kangaroos. " sound="nj_08a_07.mp3" from="  2:37.562" to="  2:39.968" allWordsNum="7"/>
				<subrecord rec_id="40" rec_time="3" name="We're done for.' " sound="nj_08a_07.mp3" from="  2:40.029" to="  2:41.313" allWordsNum="3"/>
			</record>
			<record rec_id="13" rec_time="30" sound="nj_08a_07.mp3">
				<subrecord rec_id="41" rec_time="3" name="'Don't panic, Peters,' I said. " sound="nj_08a_07.mp3" from="  2:41.864" to="  2:44.208" allWordsNum="5"/>
				<subrecord rec_id="42" rec_time="8" name="'We can't get out on the kangaroos but we can get out in them. " sound="nj_08a_07.mp3" from="  2:44.514" to="  2:49.224" allWordsNum="14"/>	
				<subrecord rec_id="43" rec_time="5" name="Here, get inside this one.' " sound="nj_08a_07.mp3" from="  2:49.448" to="  2:51.752" allWordsNum="5"/>
				<subrecord rec_id="44" rec_time="6" name="Peters climbed into the giant kangaroo's pocket. " sound="nj_08a_07.mp3" from="  2:52.180" to="  2:55.523" allWordsNum="7"/>
				<subrecord rec_id="45" rec_time="6" name="A few moments later, he escaped through the door to freedom. " sound="nj_08a_07.mp3" from="  2:55.748" to="  2:59.621" allWordsNum="11"/>			
			</record>
      <record rec_id="14" rec_time="25" sound="nj_08a_07.mp3">
				<subrecord rec_id="46" rec_time="14" name="'That's how we all got free,' said Captain King, 'and back to our spaceship and, finally, back to the safety of the Earth.' " sound="nj_08a_07.mp3" from="  3:00.783" to="  3:08.530" allWordsNum="23"/>
			</record>
      <record rec_id="15" rec_time="25" sound="nj_08a_07.mp3">
				<subrecord rec_id="47" rec_time="5" name="'What happened to Gork?' asked someone. " sound="nj_08a_07.mp3" from="  3:09.285" to="  3:12.057" allWordsNum="6"/>
			</record>
      <record rec_id="16" rec_time="25" sound="nj_08a_07.mp3">
				<subrecord rec_id="48" rec_time="5" name="'Gork? I don't know. " sound="nj_08a_07.mp3" from="  3:12.404" to="  3:14.810" allWordsNum="4"/>
				<subrecord rec_id="49" rec_time="6" name="Maybe he's still in his cave——looking for Nobody.' " sound="nj_08a_07.mp3" from="  3:14.993" to="  3:18.377" allWordsNum="9"/>
			</record>
			<record rec_id="17" rec_time="25" sound="nj_08a_07.mp3">
			<subrecord rec_id="50" rec_time="10" name="Just then, a huge figure was moving silently towards the captain's house ... " sound="nj_08a_07.mp3" from="  3:19.274" to="  3:24.697" allWordsNum="13"/>
			</record>
			<exercise type="4" index="0"/>
			</part>
	</texts>
