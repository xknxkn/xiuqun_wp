		<part part_index="0" name="The Great Wave of 2040" page="31">
			<record rec_id="0" rec_time="30" sound="nj_09b_02.mp3">
			<subrecord rec_id="1" rec_time="8" name="I have always been frightened of water. Last week in Geography, we learnt that if all the ice in Antarctica melted, a great wave, thirty metres high, would hit our city. " sound="nj_09b_02.mp3" from="0:56.253" to="1:09.057" allWordsNum="31"/>
			</record>
			<record rec_id="1" rec_time="45" sound="nj_09b_02.mp3">
			<subrecord rec_id="2" rec_time="5" name="I started to worry. " sound="nj_09b_02.mp3" from="1:09.244" to="1:11.323" allWordsNum="4"/>
			<subrecord rec_id="3" rec_time="12" name="What would happen then? People would run to high ground ... Perhaps my family and I would be lucky because we live in a high flat. " sound="nj_09b_02.mp3" from="1:11.463" to="1:21.136" allWordsNum="26"/>
			</record>
			<record rec_id="2" rec_time="45" sound="nj_09b_02.mp3">
			<subrecord rec_id="4" rec_time="6" name="Last night, I had a dream about the Great Wave ... " sound="nj_09b_02.mp3" from=" 1:21.487" to=" 1:25.506" allWordsNum="11"/>
			</record>
			<record rec_id="3" rec_time="45" sound="nj_09b_02.mp3">
			<subrecord rec_id="5" rec_time="8" name="In my dream, water was pouring everywhere. It was pouring into Luck Valley. " sound="nj_09b_02.mp3" from=" 1:25.692" to="1:32.024" allWordsNum="13"/>
			<subrecord rec_id="6" rec_time="9" name="I could see it from my window as it crossed the railway and covered the sports field at the nearby university. " sound="nj_09b_02.mp3" from="1:32.071" to="1:38.543" allWordsNum="21"/>
			<subrecord rec_id="7" rec_time="9" name="At the end of the valley, the water went up the hills, and some of it even went into the tunnel. " sound="nj_09b_02.mp3" from="1:39.034" to="1:45.716" allWordsNum="21"/>
			</record>
			<record rec_id="4" rec_time="60" sound="nj_09b_02.mp3">
			<subrecord rec_id="8" rec_time="4" name="But I couldn't just stand here!" sound="nj_09b_02.mp3" from="1:46.183" to="1:48.333" allWordsNum="8"/>
			<subrecord rec_id="9" rec_time="4" name="I must do something!" sound="nj_09b_02.mp3" from="1:48.356" to="1:49.805" allWordsNum="4"/>
            <subrecord rec_id="10" rec_time="4" name="Yes, of course!" sound="nj_09b_02.mp3" from="1:49.992" to="1:51.557" allWordsNum="3"/>
			<subrecord rec_id="11" rec_time="4" name="I used my mobile phone to e-mail my dad at work. " sound="nj_09b_02.mp3" from="1:51.978" to=" 1:55.553" allWordsNum="11"/>
			<subrecord rec_id="12" rec_time="4" name="He worked at an import-export  business. " sound="nj_09b_02.mp3" from="1:55.810" to="1:58.240" allWordsNum="6"/>
			<subrecord rec_id="13" rec_time="4" name="I wrote, 'Dad, there's a flood, what can we do?'" sound="nj_09b_02.mp3" from="1:58.497" to="2:02.913" allWordsNum="14"/>
			<subrecord rec_id="14" rec_time="4" name="I got an e-mail back from Dad. " sound="nj_09b_02.mp3" from="2:03.707" to="2:05.950" allWordsNum="7"/>
			<subrecord rec_id="15" rec_time="4" name="It said, 'I'm very busy now. " sound="nj_09b_02.mp3" from="2:06.160" to="2:08.590" allWordsNum="9"/>
			<subrecord rec_id="16" rec_time="7" name="I have just exported a lot of fish to the USA and made a big profit...'" sound="nj_09b_02.mp3" from="2:08.754" to=" 2:13.660" allWordsNum="18"/>
			</record>
			<record rec_id="5" rec_time="50" sound="nj_09b_02.mp3">
			<subrecord rec_id="17" rec_time="3" name="I tried our school website. " sound="nj_09b_02.mp3" from="2:14.221" to="2:16.277" allWordsNum="5"/>
			<subrecord rec_id="18" rec_time="5" name="'What are you doing about the flood?' I asked. " sound="nj_09b_02.mp3" from="2:16.815" to="2:19.712" allWordsNum="11"/>				
      <subrecord rec_id="19" rec_time="3" name="The reply came. " sound="nj_09b_02.mp3" from="2:19.922" to="2:21.418" allWordsNum="3"/>
			<subrecord rec_id="20" rec_time="8" name="'Our school has more students than ever. Last year... 'I stopped reading. " sound="nj_09b_02.mp3" from="2:21.534" to="2:27.259" allWordsNum="15"/>
			<subrecord rec_id="21" rec_time="7" name="I shouted, 'The WATER! Look at the WATER!'" sound="nj_09b_02.mp3" from="2:27.492" to="2:31.371" allWordsNum="10"/>
			</record>
			<record rec_id="6" rec_time="60" sound="nj_09b_02.mp3">
			<subrecord rec_id="22" rec_time="3" name="But nobody looked. " sound="nj_09b_02.mp3" from="2:31.628" to="2:33.357" allWordsNum="3"/>
			<subrecord rec_id="23" rec_time="5" name="Everywhere, people just sat at their computers. " sound="nj_09b_02.mp3" from="2:33.614" to="2:37.072" allWordsNum="7"/>
			<subrecord rec_id="24" rec_time="8" name="They sat at their computers in offices, in shops and in homes. " sound="nj_09b_02.mp3" from="2:37.539" to="2:42.306" allWordsNum="12"/>
			<subrecord rec_id="25" rec_time="5" name="They did not want to leave them. " sound="nj_09b_02.mp3" from="2:42.563" to="2:44.408" allWordsNum="7"/>
			<subrecord rec_id="26" rec_time="5" name="Computers were more important than the flood. " sound="nj_09b_02.mp3" from="2:44.899" to="2:47.983" allWordsNum="7"/>
			</record>
			<record rec_id="7" rec_time="60" sound="nj_09b_02.mp3">
			<subrecord rec_id="27" rec_time="5" name="Very soon, the water came into people's rooms. " sound="nj_09b_02.mp3" from="2:48.170" to="2:51.862" allWordsNum="10"/>
      <subrecord rec_id="28" rec_time="3" name="It poured onto their desks. " sound="nj_09b_02.mp3" from="2:52.142" to="2:53.824" allWordsNum="5"/>
			<subrecord rec_id="29" rec_time="4" name="All the hi-tech machines got wet. " sound="nj_09b_02.mp3" from=" 2:54.152" to="2:56.792" allWordsNum="6"/>
			<subrecord rec_id="30" rec_time="5" name="Finally, it covered the computers. " sound="nj_09b_02.mp3" from="2:57.119" to="2:59.946" allWordsNum="5"/>
			<subrecord rec_id="31" rec_time="4" name="Only then did people notice the flood. " sound="nj_09b_02.mp3" from="3:00.367" to="3:03.381" allWordsNum="7"/>
			<subrecord rec_id="32" rec_time="3" name="But it was too late. " sound="nj_09b_02.mp3" from="3:04.012" to="3:05.600" allWordsNum="5"/>
			<subrecord rec_id="33" rec_time="6" name="Even with all the computers in our city, the flood couldn't be stopped. " sound="nj_09b_02.mp3" from="3:06.488" to="3:11.114" allWordsNum="15"/>
			</record>
			<record rec_id="8" rec_time="60" sound="nj_09b_02.mp3">
			<subrecord rec_id="34" rec_time="3" name="And then I opened my eyes. " sound="nj_09b_02.mp3" from="3:11.465" to="3:13.801" allWordsNum="6"/>
			<subrecord rec_id="35" rec_time="3" name="There was no water. " sound="nj_09b_02.mp3" from="3:13.988" to="3:15.764" allWordsNum="4"/>
			<subrecord rec_id="36" rec_time="3" name="The year was not 2040. " sound="nj_09b_02.mp3" from="3:15.951" to="3:18.171" allWordsNum="5"/>				
      <subrecord rec_id="37" rec_time="3" name="It was 2011. " sound="nj_09b_02.mp3" from="3:18.334" to="3:20.040" allWordsNum="3"/>
			<subrecord rec_id="38" rec_time="10" name="I remembered that our city was protected by some sea walls 50 metres high and felt safe. " sound="nj_09b_02.mp3" from="3:20.437" to="3:26.769" allWordsNum="17"/>
			</record>
			<record rec_id="9" rec_time="25" sound="nj_09b_02.mp3">
			<subrecord rec_id="39" rec_time="6" name="But would a flood really happen like this one day?" sound="nj_09b_02.mp3" from="3:27.376" to="3:30.647" allWordsNum="10"/>
			<subrecord rec_id="40" rec_time="12" name="And would we be so busy with our computers that we would do nothing to save ourselves?" sound="nj_09b_02.mp3" from="3:31.045" to="3:35.904" allWordsNum="17"/>
			</record>
			<exercise type="4" index="0"/>
