	<texts>
		<part part_index="0" name="Computer facts" page="55">
			<record rec_id="0" rec_time="25" sound="nj_09a_04.mp3">
				<subrecord rec_id="1" rec_time="5" name="Smaller and smaller. " sound="nj_09a_04.mp3" from="  0:17.942" to="  0:20.453" allWordsNum="3"/>
				</record>
			<record rec_id="1" rec_time="60" sound="nj_09a_04.mp3">
				<subrecord rec_id="2" rec_time="6" name="The first computers were built in the 1940s. " sound="nj_09a_04.mp3" from="  0:21.154" to="  0:24.768" allWordsNum="8"/>
				<subrecord rec_id="3" rec_time="5" name="They were bigger than cars!" sound="nj_09a_04.mp3" from="  0:24.792" to="  0:26.873" allWordsNum="5"/>
				<subrecord rec_id="4" rec_time="6" name="Since then computers have become smaller and smaller. " sound="nj_09a_04.mp3" from="  0:27.124" to="  0:30.514" allWordsNum="8"/>
				<subrecord rec_id="5" rec_time="6" name="Desktop computers are the size of televisions. " sound="nj_09a_04.mp3" from="  0:30.630" to="  0:33.655" allWordsNum="7"/>
				<subrecord rec_id="6" rec_time="5" name="Laptops are the size of big books. " sound="nj_09a_04.mp3" from="  0:33.770" to="  0:36.159" allWordsNum="7"/>
				<subrecord rec_id="7" rec_time="5" name="Now we have palmtop computers. " sound="nj_09a_04.mp3" from="  0:36.275" to="  0:38.741" allWordsNum="5"/>
			</record>
			<record rec_id="2" rec_time="60" sound="nj_09a_04.mp3">
				<subrecord rec_id="8" rec_time="6" name="They are small enough to put in your pocket like a bar of chocolate. " sound="nj_09a_04.mp3" from="  0:38.876" to="  0:42.151" allWordsNum="14"/>
				<subrecord rec_id="9" rec_time="6" name="Other kinds of computers are so tiny that you may be unaware of them. " sound="nj_09a_04.mp3" from="  0:42.594" to="  0:46.582" allWordsNum="14"/>
				<subrecord rec_id="10" rec_time="8" name="There is probably one inside your TV, telephone, or washing machine. " sound="nj_09a_04.mp3" from="  0:47.006" to="  0:51.321" allWordsNum="11"/>
				<subrecord rec_id="11" rec_time="6" name="So you probably depend on computers more than you realize. " sound="nj_09a_04.mp3" from="  0:51.610" to="  0:55.290" allWordsNum="10"/>
				</record>
			<record rec_id="3" rec_time="25" sound="nj_09a_04.mp3">
				<subrecord rec_id="12" rec_time="6" name="What kind of jobs can a computer do?" sound="nj_09a_04.mp3" from="  0:55.560" to="  0:58.565" allWordsNum="8"/>
				</record>
			<record rec_id="4" rec_time="60" sound="nj_09a_04.mp3">
				<subrecord rec_id="13" rec_time="6" name="It is common knowledge that computers are super calculators. " sound="nj_09a_04.mp3" from="  0:58.797" to="  1:02.534" allWordsNum="9"/>
				<subrecord rec_id="14" rec_time="6" name="They can calculate very fast and rarely give wrong answers. " sound="nj_09a_04.mp3" from="  1:02.592" to="  1:06.541" allWordsNum="10"/>
				<subrecord rec_id="15" rec_time="6" name="They can also type, print and draw things. " sound="nj_09a_04.mp3" from="1:06.676" to="1:09.740" allWordsNum="8"/>
				<subrecord rec_id="16" rec_time="5" name="They can teach you and play games with you. " sound="nj_09a_04.mp3" from="  1:10.009" to="1:12.437" allWordsNum="9"/>
				<subrecord rec_id="17" rec_time="10" name="More importantly, they can operate railways and fly aeroplanes and spaceships. " sound="nj_09a_04.mp3" from="  1:12.726" to="  1:17.928" allWordsNum="11"/>
				<subrecord rec_id="18" rec_time="6" name="For these reasons, we often call them electronic brains. " sound="nj_09a_04.mp3" from="  1:18.429" to="  1:22.262" allWordsNum="9"/>
				</record>
			<record rec_id="5" rec_time="25" sound="nj_09a_04.mp3">
				<subrecord rec_id="19" rec_time="5" name="Is a computer cleverer than I am ?" sound="nj_09a_04.mp3" from="  1:22.436" to="  1:25.056" allWordsNum="7"/>
				</record>
			<record rec_id="6" rec_time="60" sound="nj_09a_04.mp3">
				<subrecord rec_id="20" rec_time="8" name="The answer to this question is, for the time being, 'No'. " sound="nj_09a_04.mp3" from="  1:25.672" to="  1:30.200" allWordsNum="11"/>
				<subrecord rec_id="21" rec_time="10" name="Your brain can understand the meaning of things better and create new ideas. " sound="nj_09a_04.mp3" from="  1:30.335" to="  1:35.382" allWordsNum="13"/>
				<subrecord rec_id="22" rec_time="12" name="However, one day computers may be able to do most things that a human brain can do and even do them better. " sound="nj_09a_04.mp3" from="  1:35.652" to="  1:41.952" allWordsNum="22"/>
				<subrecord rec_id="23" rec_time="6" name="For example, they may be better than doctors at doing their job. " sound="nj_09a_04.mp3" from="  1:42.992" to="  1:46.865" allWordsNum="12"/>
			</record>
			<record rec_id="7" rec_time="60" sound="nj_09a_04.mp3">
				<subrecord rec_id="24" rec_time="6" name="If this is true, it raises some interesting questions. " sound="nj_09a_04.mp3" from="  1:47.539" to="  1:51.354" allWordsNum="9"/>
				<subrecord rec_id="25" rec_time="6" name="What will happen to us if computers can do our jobs?" sound="nj_09a_04.mp3" from="  1:52.028" to="  1:55.650" allWordsNum="11"/>
				<subrecord rec_id="26" rec_time="3" name="How will we spend our lives?" sound="nj_09a_04.mp3" from="  1:56.055" to="  1:57.846" allWordsNum="6"/>
				<subrecord rec_id="27" rec_time="3" name="Will we have nothing to do?" sound="nj_09a_04.mp3" from="  1:58.020" to="  1:59.773" allWordsNum="6"/>
				<subrecord rec_id="28" rec_time="8" name="Computers may change our lives, but will they make them better?" sound="nj_09a_04.mp3" from="  2:00.293" to="  2:04.377" allWordsNum="11"/>
			</record>
			<record rec_id="8" rec_time="25" sound="nj_09a_04.mp3">
				<subrecord rec_id="29" rec_time="3" name="Talk to your computer. " sound="nj_09a_04.mp3" from="  2:04.724" to="  2:06.516" allWordsNum="4"/>
				</record>
			<record rec_id="9" rec_time="60" sound="nj_09a_04.mp3">
				<subrecord rec_id="30" rec_time="6" name="Now you do not even need to know how to type. " sound="nj_09a_04.mp3" from="  2:07.460" to="  2:10.735" allWordsNum="11"/>
				<subrecord rec_id="31" rec_time="6" name="You can buy a program which helps the computer recognize your voice. " sound="nj_09a_04.mp3" from="  2:11.024" to="  2:14.800" allWordsNum="12"/>
				<subrecord rec_id="32" rec_time="8" name="You can talk to the computer and it will obey your commands, and write your letters!" sound="nj_09a_04.mp3" from="  2:14.954" to="  2:19.771 " allWordsNum="16"/>
			</record>
			<record rec_id="10" rec_time="25" sound="nj_09a_04.mp3">
				<subrecord rec_id="33" rec_time="5" name="CD-ROMs/DVD-ROMs. " sound="nj_09a_04.mp3" from="  2:19.867" to="  2:23.142" allWordsNum="2"/>
				</record>
			<record rec_id="11" rec_time="60" sound="nj_09a_04.mp3">
				<subrecord rec_id="34" rec_time="8" name="Many of today's computers have CD-ROM or DVD-ROM drives. " sound="nj_09a_04.mp3" from="  2:23.875" to="  2:28.498" allWordsNum="9"/>
				<subrecord rec_id="35" rec_time="14" name="A DVD-ROM can hold millions of pages of writing, and can also contain pictures, videos and sounds. " sound="nj_09a_04.mp3" from="  2:28.633" to=" 2:35.858" allWordsNum="17"/>
				<subrecord rec_id="36" rec_time="20" name="For example, you can use your computer to read about a famous person from history and you can see a photograph or video of him or her, and even listen to them speaking. " sound="nj_09a_04.mp3" from="  2:36.417" to="  2:46.531" allWordsNum="33"/>
				<subrecord rec_id="37" rec_time="10" name="Some people think that DVD-ROMs will soon be more popular than books. " sound="nj_09a_04.mp3" from="  2:46.936" to="  2:51.983" allWordsNum="12"/>
			</record>
			<exercise type="4" index="0"/>
		</part>
			<part part_index="1" name="The history of the Internet" page="66">
			<record rec_id="0" rec_time="60" sound="nj_09a_04.mp3">
				<subrecord rec_id="1" rec_time="14" name="A research team in the United States first developed the concept of linking computers together in the 1960s. " sound="nj_09a_04.mp3" from="  6:08.071" to="  6:15.353" allWordsNum="18"/>
				<subrecord rec_id="2" rec_time="16" name="They wanted to have a system that would enable universities, research departments and the army to share resources with each other. " sound="nj_09a_04.mp3" from="  6:15.931" to="  6:24.023" allWordsNum="21"/>
			</record>
			<record rec_id="1" rec_time="60" sound="nj_09a_04.mp3">
				<subrecord rec_id="3" rec_time="10" name="Computers at this time were as large as refrigerators and could not be transported easily. " sound="nj_09a_04.mp3" from="  6:24.697" to="  6:30.265" allWordsNum="15"/>
				<subrecord rec_id="4" rec_time="10" name="Another problem was that they were often complicated and not compatible with each other. " sound="nj_09a_04.mp3" from="  6:30.650" to="  6:35.659" allWordsNum="14"/>
				<subrecord rec_id="5" rec_time="12" name="The research team had to overcome these difficulties to create a network that could share computing power. " sound="nj_09a_04.mp3" from="  6:36.257" to="  6:42.306" allWordsNum="17"/>
			</record>
			<record rec_id="2" rec_time="60" sound="nj_09a_04.mp3">
				<subrecord rec_id="6" rec_time="6" name="Gradually, more and more scholars came online. " sound="nj_09a_04.mp3" from="  6:43.790" to="  6:47.007" allWordsNum="7"/>
				<subrecord rec_id="7" rec_time="8" name="Years later, businessmen began using the Internet too. " sound="nj_09a_04.mp3" from="  6:47.392" to="  6:51.457" allWordsNum="8"/>
				<subrecord rec_id="8" rec_time="10" name="The number of users is increasing all the time and it is becoming more widely available across the world. " sound="nj_09a_04.mp3" from="  6:51.689" to="  6:58.123" allWordsNum="19"/>
				<subrecord rec_id="9" rec_time="10" name="In fact, it is possible to use the Internet somewhere in every continent in the world. " sound="nj_09a_04.mp3" from="  6:58.470" to="  7:04.057" allWordsNum="16"/>
				<subrecord rec_id="10" rec_time="12" name="At the same time, the computers are becoming smaller and smaller, and more convenient to carry around. " sound="nj_09a_04.mp3" from="  7:04.443" to="  7:10.858" allWordsNum="17"/>
				</record>
			<record rec_id="3" rec_time="60" sound="nj_09a_04.mp3">
				<subrecord rec_id="11" rec_time="5" name="There is a wide range of services available on the Internet. " sound="nj_09a_04.mp3" from="  7:11.995" to="  7:14.981" allWordsNum="11"/>
				<subrecord rec_id="12" rec_time="6" name="This means that there are many different ways to communicate. " sound="nj_09a_04.mp3" from="  7:15.174" to="  7:18.738" allWordsNum="10"/>
				<subrecord rec_id="13" rec_time="12" name="One of the most popular ways is electronic mail more commonly called 'e-mail'. " sound="nj_09a_04.mp3" from="  7:19.046" to="  7:24.884" allWordsNum="13"/>
				</record>
			<record rec_id="4" rec_time="60" sound="nj_09a_04.mp3">
				<subrecord rec_id="14" rec_time="10" name="Another popular way is browsing on the World Wide Web(WWW). " sound="nj_09a_04.mp3" from="  7:25.519" to="  7:30.606" allWordsNum="10"/>
				<subrecord rec_id="15" rec_time="12" name="The WWW is a very big series of connected pages containing various forms of multimedia. " sound="nj_09a_04.mp3" from="  7:30.933" to="  7:37.464" allWordsNum="15"/>
				<subrecord rec_id="16" rec_time="14" name="It is quite popular to use online messenger services to send instant messages to people all over the world. " sound="nj_09a_04.mp3" from="  7:37.946" to="  7:44.997" allWordsNum="19"/>
			</record>
